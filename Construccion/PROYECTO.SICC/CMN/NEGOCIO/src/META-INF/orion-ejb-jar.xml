<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE orion-ejb-jar PUBLIC "-//Evermind//DTD Enterprise JavaBeans 1.1 runtime//EN" "http://xmlns.oracle.com/ias/dtds/orion-ejb-jar.dtd">
<orion-ejb-jar>
  <enterprise-beans>
    <session-deployment name="MONBatchEJB"/>
    <session-deployment name="MONBusquedaGenerica"/>
    <session-deployment name="MONValidacion"/>
    <session-deployment name="MONAuditoria"/>
    <session-deployment name="MONTest"/>
    <session-deployment name="MONI18n"/>
    <session-deployment name="MONServicioCorreoElectronico"/>
    <!-- BLOQUE DEL GESTOR DE INTERFACES -->
    <session-deployment name="MONGestorInterfaces"/>
    <!-- FIN DEL BLOQUE DEL GESTOR DE INTERFACES -->
    <session-deployment name="MONI18nMMG"/>
  </enterprise-beans>
  <!-- BLOQUE DEL GESTOR DE INTERFACES -->
  <assembly-descriptor>
    <default-method-access>
      <security-role-mapping impliesAll="true" name="&lt;default-ejb-caller-role>"/>
    </default-method-access>
  </assembly-descriptor>
  <!-- FIN DEL BLOQUE DEL GESTOR DE INTERFACES -->
</orion-ejb-jar>
