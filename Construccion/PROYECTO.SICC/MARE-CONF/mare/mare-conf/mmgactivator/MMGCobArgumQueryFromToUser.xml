<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando CobArgum.QueryFromToUser...SC");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("cobArgumFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("cobArgumTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.CobArgumData</args-types>
		<args-types>es.indra.belcorp.mso.CobArgumData</args-types>
		<args-types>java.util.HashMap</args-types>
        <class-name>es.indra.sicc.logicanegocio.cob.MONGuiaArgumenDetal</class-name>
        <home-class>es.indra.sicc.logicanegocio.cob.MONGuiaArgumenDetalHome</home-class>
        <jndi-name>MONGuiaArgumenDetal</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("CobArgum.QueryFromToUser invocado correctamente.SC");
    </post-activation-actions>
</ejb-operation>
