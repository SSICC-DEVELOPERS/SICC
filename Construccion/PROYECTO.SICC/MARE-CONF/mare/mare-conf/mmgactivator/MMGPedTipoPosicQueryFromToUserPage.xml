<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando PedTipoPosic.QueryFromToUserPage...");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pedTipoPosicFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pedTipoPosicTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pageCount")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pageSize")</args-values>
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.PedTipoPosicData</args-types>
		<args-types>es.indra.belcorp.mso.PedTipoPosicData</args-types>
		<args-types>java.util.HashMap</args-types>
		<args-types>java.lang.Integer</args-types>
		<args-types>java.lang.Integer</args-types>
        <class-name>es.indra.sicc.logicanegocio.ped.MONTipoPosicion</class-name>
        <home-class>es.indra.sicc.logicanegocio.ped.MONTipoPosicionHome</home-class>
        <jndi-name>MONTipoPosicion</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("PedTipoPosic.QueryFromToUserPage invocado correctamente");
    </post-activation-actions>
</ejb-operation>
