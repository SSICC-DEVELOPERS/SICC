<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando MaeEncueNseView.QueryFromToUser...");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("maeEncueNseViewFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("maeEncueNseViewTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.MaeEncueNseViewData</args-types>
		<args-types>es.indra.belcorp.mso.MaeEncueNseViewData</args-types>
		<args-types>java.util.HashMap</args-types>
        <class-name>es.indra.sicc.logicanegocio.mae.MONPreguntasEncuesta</class-name>
        <home-class>es.indra.sicc.logicanegocio.mae.MONPreguntasEncuestaHome</home-class>
        <jndi-name>MONPreguntasEncuesta</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("MaeEncueNseView.QueryFromToUser invocado correctamente");
    </post-activation-actions>
</ejb-operation>
