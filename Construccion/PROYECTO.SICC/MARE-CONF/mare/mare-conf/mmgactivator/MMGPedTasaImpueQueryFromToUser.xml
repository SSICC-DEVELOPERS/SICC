<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando PedTasaImpue.QueryFromToUser...");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pedTasaImpueFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pedTasaImpueTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.PedTasaImpueData</args-types>
		<args-types>es.indra.belcorp.mso.PedTasaImpueData</args-types>
		<args-types>java.util.HashMap</args-types>
        <class-name>es.indra.sicc.logicanegocio.ped.MONImpuestosGenerales</class-name>
        <home-class>es.indra.sicc.logicanegocio.ped.MONImpuestosGeneralesHome</home-class>
        <jndi-name>MONImpuestosGenerales</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("PedTasaImpue.QueryFromToUser invocado correctamente");
    </post-activation-actions>
</ejb-operation>
