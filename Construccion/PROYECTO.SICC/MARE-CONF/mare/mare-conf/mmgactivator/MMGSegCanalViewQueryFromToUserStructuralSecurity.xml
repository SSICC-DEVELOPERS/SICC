<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando SegCanalView.QueryFromToUser...");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("segCanalViewFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("segCanalViewTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
   	<args-values>dts.getProperty("DTO_INPUT").getProperty("applyStructuralSecurity")</args-values>
    
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.SegCanalViewData</args-types>
		<args-types>es.indra.belcorp.mso.SegCanalViewData</args-types>
		<args-types>java.util.HashMap</args-types>
		<args-types>java.lang.Boolean</args-types>
        <class-name>es.indra.sicc.logicanegocio.seg.MONCanal</class-name>
        <home-class>es.indra.sicc.logicanegocio.seg.MONCanalHome</home-class>
        <jndi-name>MONCanal</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("SegCanalView.QueryFromToUser invocado correctamente");
    </post-activation-actions>
</ejb-operation>

