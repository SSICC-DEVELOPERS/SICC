NTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="ZONEUSER" DataType="STRING" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="NIVEL" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/></FilterDefinitions><DataSourceDefinitions><DataSourceDefinition Name="RFD-008" SourceType="SQL" Expression="   SELECT * FROM (   SELECT SOC.VAL_DENO SOCIEDAD_DEFECTO  FROM PRINCIPALS PRI,   PROPERTYVALUES PRO,   SEG_SOCIE SOC     WHERE PRI.NAME = %value_ZONEUSERVALUE%    AND PRO.IDPROPERTY = 48    AND PRO.IDPRINCIPAL = PRI.IDPRINCIPAL    AND PRO.STRINGVALUE = SOC.OID_SOCI ),  (  SELECT REGION,      ZONA,      SECCION,      TERRITORIO,      COD_CLIENTE,      NOMBRE,      TELEFONO,      CAMP_INGRESO,      NIVEL_CAPACI,      CONSTANCIA,      PERIODO_1,      PERIODO_2,      PERIODO_3,      PERIODO_4,       OBSERVACIONES,            INDICADOR_MONTO_DEUDA,     GANANCIA_ACUM,           DECODE(N_PEDIDOS, 0,0, VTA_ACUMULADA/N_PEDIDOS) PMNP,            VTA_PUB_P1,      VTA_PUB_P2,      VTA_PUB_P3,      VTA_PUB_P4,      DECODE(DECODE(N_PEDIDOS, 0,0, VTA_ACUMULADA/N_PEDIDOS), 0, 0, VTA_CAMP_ANALISIS/(VTA_ACUMULADA/N_PEDIDOS)) PORC_VTA_VS_PROME,      MONTO_DEUDA,      VTA_ACUMULADA,      N_PEDIDOS,      VTA_CAMP_ANALISIS      FROM  (   SELECT REGION,       ZONA,       SECCION,       TERRITORIO,       COD_CLIENTE,       NOMBRE,       TELEFONO,       CAMP_INGRESO,       NIVEL_CAPACI,              CASE WHEN (PERIODO_ULTIMO_PEDIDO IS NOT NULL) THEN         (         SELECT COUNT(DISTINCT SOL_CAB.PERD_OID_PERI)      FROM PED_SOLIC_CABEC SOL_CAB,        CRA_PERIO P      WHERE P.OID_PERI = SOL_CAB.PERD_OID_PERI          AND P.FEC_INIC >= FEC_P_18         AND P.FEC_FINA &lt;= FEC_PERIODO_ULTIMO_PEDIDO                     AND SOL_CAB.CLIE_OID_CLIE = CLIENTE                      AND SOL_CAB.IND_TS_NO_CONSO &lt;> 1         AND SOL_CAB.IND_OC = 1         AND SOL_CAB.IND_PEDI_PRUE &lt;> 1         AND SOL_CAB.FEC_FACT IS NOT NULL         AND P.PAIS_OID_PAIS =  %value_PAISVALUE%           AND P.MARC_OID_MARC = %value_MARCAVALUE%         AND P.CANA_OID_CANA = %value_CANALVALUE%        GROUP BY SOL_CAB.CLIE_OID_CLIE         )       END CONSTANCIA,          GANANCIA_ACUM,                             CASE WHEN ((OID_CAMP_INGRESO IS NOT NULL) AND (PERIODO_ULTIMO_PEDIDO IS NOT NULL)) THEN         (         SELECT COUNT(*)         FROM DTR_SEGME_CLIEN SG,           CRA_PERIO P         WHERE FEC_P_18 &lt;= P.FEC_INIC            AND FEC_PERIODO_ULTIMO_PEDIDO >= P.FEC_FINA         AND P.OID_PERI = SG.PERD_OID_PERI         AND SG.CLIE_OID_CLIE = CLIENTE         AND P.PAIS_OID_PAIS = %value_PAISVALUE%          AND P.MARC_OID_MARC = %value_MARCAVALUE%         AND P.CANA_OID_CANA = %value_CANALVALUE%           GROUP BY SG.CLIE_OID_CLIE         )       END N_PEDIDOS,                 CASE WHEN ((OID_CAMP_INGRESO IS NOT NULL) AND (PERIODO_ULTIMO_PEDIDO IS NOT NULL)) THEN         (         SELECT SUM(NVL(SG.VAL_VENT, 0))         FROM DTR_SEGME_CLIEN SG,           CRA_PERIO P         WHERE FEC_P_18 &lt;= P.FEC_INIC            AND FEC_PERIODO_ULTIMO_PEDIDO >= P.FEC_FINA         AND P.OID_PERI = SG.PERD_OID_PERI         AND SG.CLIE_OID_CLIE = CLIENTE         AND P.PAIS_OID_PAIS = %value_PAISVALUE%          AND P.MARC_OID_MARC = %value_MARCAVALUE%         AND P.CANA_OID_CANA = %value_CANALVALUE%          GROUP BY SG.CLIE_OID_CLIE         )           END VTA_ACUMULADA,          PERIODO_1,        PERIODO_2,        PERIODO_3,        PERIODO_4,        VTA_PUB_P1,        VTA_PUB_P2,        VTA_PUB_P3,        VTA_PUB_P4,        VTA_CAMP_ANALISIS,        MONTO_DEUDA,        INDICADOR_MONTO_DEUDA,        OBSERVACIONES   FROM   (    SELECT CLIE_OID_CLIE CLIENTE,        REGION,        ZONA,        SECCION,        TERRITORIO,        COD_CLIENTE,        NOMBRE,        TELEFONO,        CAMP_INGRESO,        NIVEL_CAPACI,                CASE WHEN ((OID_CAMP_INGRESO IS NOT NULL) AND (PERIODO_ULTIMO_PEDIDO IS NOT NULL)) THEN              CASE WHEN (PQ_APL_REP.FN_112_CALC_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%, OID_CAMP_INGRESO, PERIODO_ULTIMO_PEDIDO) > 18) THEN              (          SELECT FEC_INIC           FROM CRA_PERIO          WHERE OID_PERI = PQ_APL_REP.FN_008_CALC_OID_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%, PERIODO_ULTIMO_PEDIDO,18)           )                        ELSE               (             SELECT FEC_INIC            FROM CRA_PERIO            WHERE OID_PERI = OID_CAMP_INGRESO            )           END            END FEC_P_18,               CASE WHEN (PERIODO_ULTIMO_PEDIDO IS NOT NULL) THEN             (          SELECT FEC_FINA          FROM CRA_PERIO          WHERE OID_PERI = PERIODO_ULTIMO_PEDIDO          )        END FEC_PERIODO_ULTIMO_PEDIDO,        OID_CAMP_INGRESO,        PERIODO_ULTIMO_PEDIDO,               PERIODO_1,        PERIODO_2,        PERIODO_3,        PERIODO_4,        VTA_PUB_P1,        VTA_PUB_P2,        VTA_PUB_P3,        VTA_PUB_P4,        GANANCIA_ACUM,        VTA_CAMP_ANALISIS,        MONTO_DEUDA,        INDICADOR_MONTO_DEUDA,        OBSERVACIONES    FROM    (    SELECT SEG_CLI.CLIE_OID_CLIE,        REG.DES_REGI REGION,        ZONA.DES_ZONA ZONA,                (        SELECT SEC.COD_SECC        FROM MAE_CLIEN_UNIDA_ADMIN UA,          ZON_TERRI_ADMIN TER_ADM,       ZON_SECCI SEC,       ZON_ZONA Z        WHERE UA.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE           AND TER_ADM.OID_TERR_ADMI = UA.ZTAD_OID_TERR_ADMI        AND SEC.OID_SECC = TER_ADM.ZSCC_OID_SECC        AND Z.OID_ZONA = SEC.ZZON_OID_ZONA        AND Z.OID_ZONA = ZONA.OID_ZONA        AND ROWNUM = 1        AND UA.IND_ACTI = 1        ) SECCION,                (        SELECT TER.COD_TERR        FROM MAE_CLIEN_UNIDA_ADMIN UA,          ZON_TERRI_ADMIN TER_ADM,       ZON_SECCI SEC,       ZON_ZONA Z,       ZON_TERRI TER        WHERE UA.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE           AND TER_ADM.OID_TERR_ADMI = UA.ZTAD_OID_TERR_ADMI        AND SEC.OID_SECC = TER_ADM.ZSCC_OID_SECC        AND Z.OID_ZONA = SEC.ZZON_OID_ZONA        AND Z.OID_ZONA = ZONA.OID_ZONA        AND TER_ADM.TERR_OID_TERR = TER.OID_TERR        AND ROWNUM = 1         AND UA.IND_ACTI = 1           ) TERRITORIO,      CLI.COD_CLIE COD_CLIENTE,        SUBSTR(CLI.VAL_APE1||' '||CLI.VAL_APE2||' '||CLI.VAL_NOM1||' '||CLI.VAL_NOM2, 0, 40) NOMBRE,        (        SELECT CLI_COM.VAL_TEXT_COMU        FROM MAE_CLIEN_COMUN CLI_COM,          MAE_TIPO_COMUN TIP_COM        WHERE CLI_COM.CLIE_OID_CLIE = CLI.OID_CLIE           AND CLI_COM.TICM_OID_TIPO_COMU = TIP_COM.OID_TIPO_COMU          AND TIP_COM.COD_TIPO_COMU = 'TF'         ) TELEFONO,                (        SELECT PERI.OID_PERI        FROM MAE_CLIEN_HISTO_ESTAT EST,MAE_ESTAT_CLIEN EST_CLI,          CRA_PERIO PERI        WHERE EST.CLIE_OID_CLIE = CLI.OID_CLIE           AND EST_CLI.OID_ESTA_CLIE = EST.ESTA_OID_ESTA_CLIE                                              AND EST_CLI.COD_ESTA_CLIE = '02'                                                AND PERI.OID_PERI = EST.PERD_OID_PERI         AND ROWNUM = 1        ) OID_CAMP_INGRESO,                (        SELECT PERI.VAL_NOMB_PERI        FROM MAE_CLIEN_HISTO_ESTAT EST, MAE_ESTAT_CLIEN EST_CLI,          CRA_PERIO PERI        WHERE EST.CLIE_OID_CLIE = CLI.OID_CLIE          AND EST_CLI.OID_ESTA_CLIE = EST.ESTA_OID_ESTA_CLIE                                           AND EST_CLI.COD_ESTA_CLIE = '02'        AND PERI.OID_PERI = EST.PERD_OID_PERI        AND ROWNUM = 1        ) CAMP_INGRESO,                   (       SELECT GEN_TIP.VAL_I18N       FROM EDU_HISTO_CURSO HIS,               EDU_MATRI_CURSO MAT,            EDU_TIPO_CURSO TIP_COR,        (        SELECT HIS.CLIE_OID_CLIE,           MAX(HIS.FEC_ASIS) FEC_ASIS        FROM EDU_HISTO_CURSO HIS        GROUP BY HIS.CLIE_OID_CLIE        ) MAXI,        (        SELECT GEN.VAL_OID, GEN.VAL_I18N        FROM V_GEN_I18N_SICC GEN        WHERE GEN.ATTR_ENTI = 'EDU_TIPO_CURSO'          AND GEN.IDIO_OID_IDIO = %value_IDIOMAVALUE%          ) GEN_TIP       WHERE HIS.CLIE_OID_CLIE = MAXI.CLIE_OID_CLIE         AND HIS.FEC_ASIS = MAXI.FEC_ASIS                 AND HIS.MCUR_OID_CURS = MAT.OID_CURS       AND MAT.TICU_OID_TIPO_CURS = TIP_COR.OID_TIPO_CURS       AND HIS.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE       AND TIP_COR.OID_TIPO_CURS = GEN_TIP.VAL_OID(+)       ) NIVEL_CAPACI,                      (         SELECT PER.OID_PERI         FROM PED_SOLIC_CABEC SOL,           (         SELECT *          FROM CRA_PERIO PERI         WHERE PERI.PAIS_OID_PAIS = %value_PAISVALUE%              AND PERI.MARC_OID_MARC = %value_MARCAVALUE%             AND PERI.CANA_OID_CANA = %value_CANALVALUE%           ORDER BY FEC_FINA DESC        )PER         WHERE SOL.PERD_OID_PERI = PER.OID_PERI            AND SOL.CLIE_OID_CLIE = CLI.OID_CLIE         AND SOL.IND_TS_NO_CONSO &lt;> 1         AND SOL.IND_OC = 1         AND SOL.IND_PEDI_PRUE &lt;> 1         AND SOL.FEC_FACT IS NOT NULL         AND ROWNUM = 1        ) PERIODO_ULTIMO_PEDIDO,                                (        SELECT VAL_NOMB_PERI        FROM CRA_PERIO        WHERE OID_PERI = %value_PERIODODESDEVALUE%         ) PERIODO_1,                (        SELECT VAL_NOMB_PERI        FROM CRA_PERIO        WHERE OID_PERI = (         SELECT PQ_APL_REP.FN_008_CALC_OID_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%,%value_PERIODOHASTAVALUE%,3)            FROM DUAL         )        ) PERIODO_2,               (        SELECT VAL_NOMB_PERI        FROM CRA_PERIO        WHERE OID_PERI =(         SELECT PQ_APL_REP.FN_008_CALC_OID_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%,%value_PERIODOHASTAVALUE%,2)           FROM DUAL         )        ) PERIODO_3,              (        SELECT VAL_NOMB_PERI        FROM CRA_PERIO        WHERE OID_PERI = %value_PERIODOHASTAVALUE%         ) PERIODO_4,                (        SELECT NVL(SUM(SEG.VAL_VENT),0) + NVL(SUM(SEG.IMP_DES1), 0) + NVL(SUM(SEG.IMP_DES3), 0)        FROM DTR_SEGME_CLIEN SEG        WHERE SEG.PERD_OID_PERI = %value_PERIODODESDEVALUE%            AND SEG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        ) VTA_PUB_P1,             (        SELECT NVL(SUM(SEG.VAL_VENT),0) + NVL(SUM(SEG.IMP_DES1), 0) + NVL(SUM(SEG.IMP_DES3), 0)        FROM DTR_SEGME_CLIEN SEG        WHERE SEG.PERD_OID_PERI =         (         SELECT PQ_APL_REP.FN_008_CALC_OID_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%,%value_PERIODOHASTAVALUE%,3)           FROM DUAL         )               AND SEG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        ) VTA_PUB_P2,              (        SELECT NVL(SUM(SEG.VAL_VENT),0) + NVL(SUM(SEG.IMP_DES1), 0) + NVL(SUM(SEG.IMP_DES3), 0)        FROM DTR_SEGME_CLIEN SEG        WHERE SEG.PERD_OID_PERI =         (         SELECT PQ_APL_REP.FN_008_CALC_OID_PER_ANT(%value_PAISVALUE%, %value_MARCAVALUE%, %value_CANALVALUE%,%value_PERIODOHASTAVALUE%,2)           FROM DUAL         )         AND SEG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        ) VTA_PUB_P3,                 (        SELECT NVL(SUM(SEG.VAL_VENT),0) + NVL(SUM(SEG.IMP_DES1), 0) + NVL(SUM(SEG.IMP_DES3), 0)        FROM DTR_SEGME_CLIEN SEG        WHERE SEG.PERD_OID_PERI = %value_PERIODOHASTAVALUE%            AND SEG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        ) VTA_PUB_P4,              (        SELECT NVL(SUM(SEG.IMP_DES1), 0) + NVL(SUM(SEG.IMP_DES3), 0)        FROM DTR_SEGME_CLIEN SEG,           CRA_PERIO PERI,         (         SELECT FEC_INIC         FROM CRA_PERIO         WHERE OID_PERI = %value_PERIODODESDEVALUE%          ) FI,         (         SELECT FEC_FINA         FROM CRA_PERIO         WHERE OID_PERI = %value_PERIODOHASTAVALUE%          ) FF        WHERE FI.FEC_INIC &lt;= PERI.FEC_INIC        AND FF.FEC_FINA >= PERI.FEC_FINA        AND PERI.OID_PERI = SEG.PERD_OID_PERI        AND SEG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        GROUP BY SEG.CLIE_OID_CLIE                  ) GANANCIA_ACUM,              (        SELECT SUM(SG.VAL_VENT)        FROM DTR_SEGME_CLIEN SG,          CRA_PERIO P        WHERE P.OID_PERI = %value_PERIODOHASTAVALUE%           AND SG.PERD_OID_PERI = P.OID_PERI        AND SG.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE        GROUP BY SG.CLIE_OID_CLIE         ) VTA_CAMP_ANALISIS,              (        CASE WHEN (6 = %value_NIVELVALUE%) THEN              PQ_APL_REP.FN_CU_CALC_SALDO_CTA_CTE(SEG_CLI.CLIE_OID_CLIE)        END              ) MONTO_DEUDA,        (        CASE WHEN (6 = %value_NIVELVALUE%) THEN              0        ELSE             1        END              ) INDICADOR_MONTO_DEUDA,              (        SELECT VAL_TEXT        FROM MAE_CLIEN_OBSER CLI_OBS        WHERE CLI_OBS.CLIE_OID_CLIE = SEG_CLI.CLIE_OID_CLIE           AND CLI_OBS.MARC_OID_MARC = PERI.MARC_OID_MARC        ) OBSERVACIONES    FROM DTR_SEGME_CLIEN SEG_CLI,      CRA_PERIO PERI,      ZON_REGIO REG,      ZON_ZONA ZONA,      MAE_CLIEN CLI,      (      SELECT FEC_INIC      FROM CRA_PERIO       WHERE OID_PERI = %value_PERIODODESDEVALUE%       ) FI,      (      SELECT FEC_FINA      FROM CRA_PERIO       WHERE OID_PERI =  %value_PERIODOHASTAVALUE%       ) FF      WHERE SEG_CLI.PERD_OID_PERI = PERI.OID_PERI       AND FI.FEC_INIC &lt;= PERI.FEC_INIC       AND FF.FEC_FINA >= PERI.FEC_FINA       AND SEG_CLI.NVSG_OID_NIVE_SEGM = %value_NIVELVALUE%       AND CLI.OID_CLIE = SEG_CLI.CLIE_OID_CLIE       AND REG.OID_REGI = SEG_CLI.ZORG_OID_REGI                   AND ZONA.OID_ZONA = SEG_CLI.ZZON_OID_ZONA       AND REG.OID_REGI = ZONA.ZORG_OID_REGI       AND PERI.PAIS_OID_PAIS =  %value_PAISVALUE%       AND PERI.MARC_OID_MARC =  %value_MARCAVALUE%       AND PERI.CANA_OID_CANA = %value_CANALVALUE%       AND REG.PAIS_OID_PAIS =  %value_PAISVALUE%       AND REG.MARC_OID_MARC =  %value_MARCAVALUE%       AND REG.CANA_OID_CANA = %value_CANALVALUE%           %start_REGIONVALUE% AND   SEG_CLI.ZORG_OID_REGI = %value_REGIONVALUE% %end_REGIONVALUE%           %start_ZONAVALUE% AND SEG_CLI.ZZON_OID_ZONA = %value_ZONAVALUE% %end_ZONAVALUE%                          )   )     )     ORDER BY REGION,       ZONA,     SECCION,   