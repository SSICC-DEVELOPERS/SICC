tion Name="ACTI_INICIALES_REALAA" Description="ACTI_INICIALES_REALAA" AggregateType="SUM" GroupFooterType="NONE" Calculated="false" AggregateFunction="" GroupFooterFunction="" ExternalData="ACTI_INICIALES_REALAA" ExternalCalculated="false"/></MetricSourceDefinitions><FilterDefinitions><FilterDefinition Name="IDIOMA" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="PAIS" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="CANAL" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="MARCA" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="PERIODODESDE" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="PERIODOHASTA" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="REGION" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="ZONA" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="ZONEUSER" DataType="STRING" FilterType="EQUALTO" Visual="false"/></FilterDefinitions><DataSourceDefinitions><DataSourceDefinition Name="RFD-001" SourceType="SQL" Expression="    SELECT * FROM (   SELECT SOC.VAL_DENO SOCIEDAD_DEFECTO  FROM PRINCIPALS PRI,   PROPERTYVALUES PRO,   SEG_SOCIE SOC     WHERE PRI.NAME = %value_ZONEUSERVALUE%    AND PRO.IDPROPERTY = 48    AND PRO.IDPRINCIPAL = PRI.IDPRINCIPAL    AND PRO.STRINGVALUE = SOC.OID_SOCI ), ( SELECT DATOS_AAAA.PAIS,     DATOS_AAAA.DES_REGI,     DATOS_AAAA.DES_ZONA,     DATOS_AAAA.VAL_ANIO AAAA,        TRUNC(((DATOS_AAAA.COD_PERI) - 1)/6)+1 GRUPO,     DATOS_AAAA.VAL_NOMB_PERI,     DATOS_AAAA.PERI_FIN,     DATOS_AA.VENTA_NETA_ESTAD VN_REAL_AA,     DATOS_AAAA.VENTA_NETA_ESTAD VN_REAL_AAAA,     DATOS_AAAA.PLAN_VENTA_NETA_ESTA VN_PLAN_AAAA,     CASE WHEN (DATOS_AAAA.VENTA_NETA_ESTAD IS NOT NULL) THEN         DECODE(DATOS_AA.VENTA_NETA_ESTAD, 0, 0, ((DATOS_AAAA.VENTA_NETA_ESTAD / DATOS_AA.VENTA_NETA_ESTAD)-1)*100)     END VN_PORC_REALAAAA_REALAA,     CASE WHEN (DATOS_AAAA.VENTA_NETA_ESTAD IS NOT NULL) THEN     DECODE(DATOS_AAAA.PLAN_VENTA_NETA_ESTA, 0, 0, ((DATOS_AAAA.VENTA_NETA_ESTAD / DATOS_AAAA.PLAN_VENTA_NETA_ESTA)-1)*100)     END VN_PORC_REALAAAA_PLAN,     DATOS_AA.N_PEDIDO N_PEDIDO_AA,     DATOS_AAAA.N_PEDIDO N_PEDIDO_AAAA,     DATOS_AAAA.PLAN_N_PEDIDO N_PEDIDO_PLAN,     CASE WHEN (DATOS_AAAA.N_PEDIDO IS NOT NULL) THEN         DECODE(DATOS_AA.N_PEDIDO, 0, 0, ((DATOS_AAAA.N_PEDIDO / DATOS_AA.N_PEDIDO)-1)*100)     END N_PEDIDO_PORC_REALAAAA_REALAA,     CASE WHEN (DATOS_AAAA.N_PEDIDO IS NOT NULL) THEN       DECODE(DATOS_AAAA.PLAN_N_PEDIDO, 0, 0, ((DATOS_AAAA.N_PEDIDO / DATOS_AAAA.PLAN_N_PEDIDO)-1)*100)     END N_PEDIDO_PORC_REALAAAA_PLAN,     ROUND(DECODE(DATOS_AA.ACTI_FINALES, 0, 0, (DATOS_AA.N_PEDIDO/DATOS_AA.ACTI_FINALES)*100)) ACTI_PORC_REALAA,     CASE WHEN ((DATOS_AAAA.N_PEDIDO IS NOT NULL) AND (DATOS_AAAA.ACTI_FINALES IS NOT NULL)) THEN       ROUND(DECODE(DATOS_AAAA.ACTI_FINALES, 0, 0, (DATOS_AAAA.N_PEDIDO/DATOS_AAAA.ACTI_FINALES)*100))     END ACTI_PORC_REALAAAA,     ROUND(DECODE(DATOS_AAAA.PLAN_ACTI_FINALES, 0, 0, (DATOS_AAAA.PLAN_N_PEDIDO/DATOS_AAAA.PLAN_ACTI_FINALES)*100)) ACTI_PORC_PLAN_AAAA,     CASE WHEN ((DATOS_AAAA.N_PEDIDO IS NOT NULL) AND (DATOS_AAAA.ACTI_FINALES IS NOT NULL)) THEN         DECODE(ROUND(DECODE(DATOS_AA.ACTI_FINALES, 0, 0, (DATOS_AA.N_PEDIDO/DATOS_AA.ACTI_FINALES)*100)), 0, 0, ((ROUND(DECODE(DATOS_AAAA.ACTI_FINALES, 0, 0, (DATOS_AAAA.N_PEDIDO/DATOS_AAAA.ACTI_FINALES)*100)) / ROUND(DECODE(DATOS_AA.ACTI_FINALES, 0, 0, (DATOS_AA.N_PEDIDO/DATOS_AA.ACTI_FINALES)*100)))-1)*100)     END ACTI_PORC_REALAAAA_REALAA,     CASE WHEN ((DATOS_AAAA.N_PEDIDO IS NOT NULL) AND (DATOS_AAAA.ACTI_FINALES IS NOT NULL)) THEN         DECODE(ROUND(DECODE(DATOS_AAAA.PLAN_ACTI_FINALES, 0, 0, (DATOS_AAAA.PLAN_N_PEDIDO/DATOS_AAAA.PLAN_ACTI_FINALES)*100)), 0, 0, ((ROUND(DECODE(DATOS_AAAA.ACTI_FINALES, 0, 0, (DATOS_AAAA.N_PEDIDO/DATOS_AAAA.ACTI_FINALES)*100)) / ROUND(DECODE(DATOS_AAAA.PLAN_ACTI_FINALES, 0, 0, (DATOS_AAAA.PLAN_N_PEDIDO/DATOS_AAAA.PLAN_ACTI_FINALES)*100)))-1)*100)     END ACTI_PORC_REALAAAA_PLAN,     DATOS_AA.ACTI_FINALES ACTI_FINALES_REALAA,     DATOS_AAAA.ACTI_FINALES ACTI_FINALES_REALAAAA,     DATOS_AAAA.PLAN_ACTI_FINALES ACTI_FINALES_PLANAAAA,     CASE WHEN (DATOS_AAAA.ACTI_FINALES IS NOT NULL) THEN         DECODE(DATOS_AA.ACTI_FINALES, 0, 0, ((DATOS_AAAA.ACTI_FINALES / DATOS_AA.ACTI_FINALES)-1)*100)     END ACTI_FINALES_REALAAAA_REALAA,     CASE WHEN (DATOS_AAAA.ACTI_FINALES IS NOT NULL) THEN         DECODE(DATOS_AAAA.PLAN_ACTI_FINALES, 0, 0, ((DATOS_AAAA.ACTI_FINALES / DATOS_AAAA.PLAN_ACTI_FINALES)-1)*100)     END ACTI_FINALES_REALAAAA_PLAN,         ROUND(DECODE(DATOS_AA.ACTI_INICIALES, 0, 0, (DATOS_AA.ACTI_FINALES/DATOS_AA.ACTI_INICIALES)*100)) RETEN_REALAA,     CASE WHEN (DATOS_AAAA.ACTI_FINALES IS NOT NULL) THEN         ROUND(DECODE(DATOS_AAAA.ACTI_INICIALES, 0, 0, (DATOS_AAAA.ACTI_FINALES/DATOS_AAAA.ACTI_INICIALES)*100))     END RETEN_REALAAAA,     ROUND(DECODE(DATOS_AAAA.PLAN_ACTI_INICIALES, 0, 0, (DATOS_AAAA.PLAN_ACTI_FINALES/DATOS_AAAA.PLAN_ACTI_INICIALES)*100)) RETEN_PLANAAAA,     IND_PERI_FIN IND_PARA_TOTAL_CAMPANIA_FIN,     DATOS_AAAA.ACTI_INICIALES ACTI_INICIALES_REALAAAA,     DATOS_AAAA.PLAN_ACTI_INICIALES ACTI_INICIALES_PLAN,     DATOS_AA.ACTI_INICIALES ACTI_INICIALES_REALAA FROM (  SELECT V_AAAA.PAIS,      V_AAAA.OID_REGI,      V_AAAA.DES_REGI,      V_AAAA.OID_ZONA,      V_AAAA.DES_ZONA,      V_AAAA.VAL_ANIO,      SUBSTR(V_AAAA.COD_PERI,5,2) COD_PERI,      V_AAAA.VAL_NOMB_PERI,      DATOS_AAAA.VENTA_NETA_ESTAD,      DATOS_AAAA.N_PEDIDO,      DATOS_AAAA.ACTI_FINALES,      DATOS_AAAA.ACTI_INICIALES,      V_AAAA.PLAN_VENTA_NETA_ESTA,      V_AAAA.PLAN_N_PEDIDO,      V_AAAA.PLAN_ACTI_FINALES,      V_AAAA.PLAN_ACTI_INICIALES,      V_AAAA.IND_PERI_FIN,      V_AAAA.PERI_FIN  FROM  (   SELECT REG.OID_REGI,       ZONA.OID_ZONA,       PERI.OID_PERI,       NVL((       SELECT SUM(VRA.IMP_VENT_NETA_ESTA)       FROM INT_FUENT_VENTA_REAL_VACUM VRA       WHERE VRA.PAIS_OID_PAIS =   %value_PAISVALUE%            AND VRA.PERD_OID_PERI = PERI.OID_PERI         AND VRA.ZORG_OID_REGI = REG.OID_REGI            AND VRA.ZZON_OID_ZONA = ZONA.OID_ZONA            GROUP BY REG.OID_REGI,           ZONA.OID_ZONA,       PERI.OID_PERI        ), 0) VENTA_NETA_ESTAD,       NVL((       SELECT SUM(VRA.NUM_PEDI)       FROM INT_FUENT_VENTA_REAL_VACUM VRA       WHERE VRA.PAIS_OID_PAIS =   %value_PAISVALUE%            AND VRA.PERD_OID_PERI = PERI.OID_PERI         AND VRA.ZORG_OID_REGI = REG.OID_REGI            AND VRA.ZZON_OID_ZONA = ZONA.OID_ZONA            GROUP BY REG.OID_REGI,           ZONA.OID_ZONA,        PERI.OID_PERI        ), 0) N_PEDIDO,       NVL((       SELECT SUM(VR.NUM_ACTI_FINA)       FROM INT_FUENT_VENTAS_REAL VR       WHERE VR.PAIS_OID_PAIS =   %value_PAISVALUE%            AND VR.PERD_OID_PERI = PERI.OID_PERI         AND VR.ZORG_OID_REGI = REG.OID_REGI            AND VR.ZZON_OID_ZONA = ZONA.OID_ZONA            GROUP BY REG.OID_REGI,           ZONA.OID_ZONA,        PERI.OID_PERI        ), 0) ACTI_FINALES,       NVL((       SELECT SUM(VR.NUM_ACTI_INIC)       FROM INT_FUENT_VENTAS_REAL VR       WHERE VR.PAIS_OID_PAIS =   %value_PAISVALUE%            AND VR.PERD_OID_PERI = PERI.OID_PERI         AND VR.ZORG_OID_REGI = REG.OID_REGI           AND VR.ZZON_OID_ZONA = ZONA.OID_ZONA           GROUP BY REG.OID_REGI,           ZONA.OID_ZONA,           PERI.OID_PERI        ), 0) ACTI_INICIALES       FROM CRA_PERIO PERI,     ZON_ZONA ZONA,     ZON_REGIO REG,     (      SELECT PER_COR.OID_PERI,        PER_COR.COD_PERI      FROM SEG_PERIO_CORPO PER_COR      WHERE PER_COR.VAL_ANIO = (               SELECT PER_COR.VAL_ANIO            FROM SEG_PERIO_CORPO PER_COR,               CRA_PERIO PERI             WHERE PERI.OID_PERI = %value_PERIODODESDEVALUE%                     AND PERI.PERI_OID_PERI = PER_COR.OID_PERI                           )      ) PC  WHERE PERI.PERI_OID_PERI = PC.OID_PERI      AND ZONA.ZORG_OID_REGI = REG.OID_REGI      AND PERI.PAIS_OID_PAIS = %value_PAISVALUE%       AND PERI.MARC_OID_MARC = %value_MARCAVALUE%       AND PERI.CANA_OID_CANA = %value_CANALVALUE%     AND ZONA.PAIS_OID_PAIS = PERI.PAIS_OID_PAIS    AND REG.PAIS_OID_PAIS = PERI.PAIS_OID_PAIS            %start_ZONAVALUE%    AND ZONA.OID_ZONA =  %value_ZONAVALUE%  %end_ZONAVALUE%    %start_REGIONVALUE%   and reg.oid_regi in  (%value_REGIONVALUE%)  %end_REGIONVALUE%     ) DATOS_AAAA,  (  SELECT GEN_PAIS.VAL_I18N PAIS,      REG.OID_REGI,      REG.DES_REGI,      ZONA.OID_ZONA,      ZONA.DES_ZONA,      PC.VAL_ANIO,      PC.COD_PERI,      PERI.OID_PERI,      PERI.VAL_NOMB_PERI,      PC.IND_PERI_FIN,      PC.PERI_FIN,      NVL((      SELECT SUM(FVP.VAL_VENT_NETA_ESTA)      FROM INT_FUENT_VENTA_PREVI_SAP FVP      WHERE FVP.PERD_OID_PERI = PERI.OID_PERI         AND FVP.ZORG_OID_REGI = REG.OID_REGI      AND FVP.ZZON_OID_ZONA = ZONA.OID_ZONA      GROUP BY REG.OID_REGI,         ZONA.OID_ZONA,      PERI.OID_PERI              ), 0) PLAN_VENTA_NETA_ESTA,      NVL((      SELECT SUM(FVP.NUM_PEDI)      FROM INT_FUENT_VENTA_PREVI_SAP FVP      WHERE FVP.PERD_OID_PERI = PERI.OID_PERI         AND FVP.ZORG_OID_REGI = REG.OID_REGI      AND FVP.ZZON_OID_ZONA = ZONA.OID_ZONA      GROUP BY REG.OID_REGI,         ZONA.OID_ZONA,      PERI.OID_PERI              ), 0) PLAN_N_PEDIDO,      NVL((      SELECT SUM(FVP.NUM_ACTI_FINA)      FROM INT_FUENT_VENTA_PREVI_SAP FVP      WHERE FVP.PERD_OID_PERI = PERI.OID_PERI         AND FVP.ZORG_OID_REGI = REG.OID_REGI      AND FVP.ZZON_OID_ZONA = ZONA.OID_ZONA      GROUP BY REG.OID_REGI,         ZONA.OID_ZONA,      PERI.OID_PERI              ), 0) PLAN_ACTI_FINALES,      NVL((      SELECT SUM(FVP.NUM_ACTI_INIC)      FROM INT_FUENT_VENTA_PREVI_SAP FVP      WHERE FVP.PERD_OID_PERI = PERI.OID_PERI         AND FVP.ZORG_OID_REGI = REG.OID_REGI      AND FVP.ZZON_OID_ZONA = ZONA.OID_ZONA      GROUP BY REG.OID_REGI,         ZONA.OID_ZONA,      PERI.OID_PERI              ), 0) PLAN_ACTI_INICIALES           FROM CRA_PERIO PERI,    ZON_ZONA ZONA,    ZON_REGIO REG,    (    SELECT PER_COR.OID_PERI,     PER_COR.COD_PERI,     PER_COR.VAL_ANIO,     CASE WHEN ( PER_COR.COD_PERI &lt;= P.COD_PERI) THEN         1     ELSE         0     END IND_PERI_FIN,     SUBSTR(P.COD_PERI, LENGTH(P.COD_PERI)-1, LENGTH(P.COD_PERI)) PERI_FIN    FROM SEG_PERIO_CORPO PER_COR,          (          SELECT PER_COR.VAL_ANIO, PER_COR.COD_PERI          FROM SEG_PERIO_CORPO PER_COR,            CRA_PERIO PERI           WHERE PERI.OID_PERI = %value_PERIODOHASTAVALUE%                    AND PERI.PERI_OID_PERI = PER_COR.OID_PERI                       ) P    WHERE PER_COR.VAL_ANIO = P.VAL_ANIO   ) PC,   (   SELECT GEN.VAL_OID, GEN.VAL_I18N   FROM V_GEN_I18N_SICC GEN   WHERE GEN.ATTR_ENTI = 'SEG_PAIS'      AND GEN.IDIO_OID_IDIO = %value_IDIOMAVALUE%            ) GEN_PAIS  WHERE PERI.PERI_OID_PERI = PC.OID_PERI      AND GEN_PAIS.VAL_OID(+) = PERI.PAIS_OID_PAIS     AND PERI.MARC_OID_MARC = %value_MARCAVALUE%       AND PERI.CANA_OID_CANA = %value_CANALVALUE%                AND PERI.PAIS_OID_PAIS = %value_PAISVALUE%               AND ZONA.ZORG_OID_REGI = REG.OID_REGI     AND ZONA.PAIS_OID_PAIS = PERI.PAIS_OID_PAIS    AND REG.PAIS_OID_PAIS = PERI.PAIS_OID_PAIS           %start_ZONAVALUE%    AND ZONA.OID_ZONA =  %value_ZONAVALUE%  %end_ZONAVALUE%    %start_REGIONVALUE%   and reg.oid_regi in  (%value_REGIONVALUE%)  %end_REGIONVALUE%    ) V_AAAA  WHERE DATOS_AAAA.OID_PERI(+) = V_AAAA.OID_PERI     AND DATOS_AAAA.OID_REGI(+) = V_AAAA.OID_REGI     AND DATOS_AAAA.OID_ZONA(+) = V_AAAA.OID_ZONA ) DATOS_AAAA, (       SELECT OID_REGI,      OID_ZONA,      SUBSTR(COD_PERI,5,2) COD_PERI,      VENTA_NETA_ESTAD,      N_PEDIDO,      ACTI_FINALES,       ACTI_INICIALES       FROM    (   SELECT REG.OID_REGI,       ZONA.OID_ZONA,       PC.COD_PERI,       NVL((        SELECT SUM(VRA.IMP_VENT_NETA_ESTA)        FROM INT_FUENT_VENTA_REAL_VACUM VRA        WHERE VRA.PAIS_OID_PAIS = %value_PAISVALUE%             AND VRA.PERD_OID_PERI