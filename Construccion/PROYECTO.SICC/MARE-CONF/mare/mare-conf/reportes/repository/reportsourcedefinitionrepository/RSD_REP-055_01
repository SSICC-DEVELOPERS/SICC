odos.CODIGO, '07', datos.por_apor_per , NULL), 0) AS DACUM_C07 ,         NVL(DECODE (periodos.CODIGO, '08', datos.por_apor_per , NULL), 0) AS DACUM_C08 ,         NVL(DECODE (periodos.CODIGO, '09', datos.por_apor_per , NULL), 0) AS DACUM_C09 ,         NVL(DECODE (periodos.CODIGO, '10', datos.por_apor_per , NULL), 0) AS DACUM_C10 ,         NVL(DECODE (periodos.CODIGO, '11', datos.por_apor_per , NULL), 0) AS DACUM_C11 ,         NVL(DECODE (periodos.CODIGO, '12', datos.por_apor_per , NULL), 0) AS DACUM_C12 ,         NVL(DECODE (periodos.CODIGO, '13', datos.por_apor_per , NULL), 0) AS DACUM_C13 ,         NVL(DECODE (periodos.CODIGO, '14', datos.por_apor_per , NULL), 0) AS DACUM_C14 ,         NVL(DECODE (periodos.CODIGO, '15', datos.por_apor_per , NULL), 0) AS DACUM_C15 ,         NVL(DECODE (periodos.CODIGO, '16', datos.por_apor_per , NULL), 0) AS DACUM_C16 ,         NVL(DECODE (periodos.CODIGO, '17', datos.por_apor_per , NULL), 0) AS DACUM_C17,         NVL(DECODE (periodos.CODIGO, '18', datos.por_apor_per , NULL), 0) AS DACUM_C18,         NVL(DECODE (periodos.CODIGO, '01', datos.monto_venta, NULL), 0) AS MV_C01 ,         NVL(DECODE (periodos.CODIGO, '02', datos.monto_venta, NULL), 0) AS MV_C02 ,         NVL(DECODE (periodos.CODIGO, '03', datos.monto_venta, NULL), 0) AS MV_C03 ,         NVL(DECODE (periodos.CODIGO, '04', datos.monto_venta, NULL), 0) AS MV_C04 ,         NVL(DECODE (periodos.CODIGO, '05', datos.monto_venta, NULL), 0) AS MV_C05 ,         NVL(DECODE (periodos.CODIGO, '06', datos.monto_venta, NULL), 0) AS MV_C06 ,         NVL(DECODE (periodos.CODIGO, '07', datos.monto_venta, NULL), 0) AS MV_C07 ,         NVL(DECODE (periodos.CODIGO, '08', datos.monto_venta, NULL), 0) AS MV_C08 ,         NVL(DECODE (periodos.CODIGO, '09', datos.monto_venta, NULL), 0) AS MV_C09 ,         NVL(DECODE (periodos.CODIGO, '10', datos.monto_venta, NULL), 0) AS MV_C10 ,         NVL(DECODE (periodos.CODIGO, '11', datos.monto_venta, NULL), 0) AS MV_C11 ,         NVL(DECODE (periodos.CODIG