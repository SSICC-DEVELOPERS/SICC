          %start_CLASIFICACIONVALUES%     AND sol.clie_oid_clie IN (                                                             SELECT subt.clie_oid_clie                                                               FROM mae_clien_tipo_subti subt,                                                                    mae_clien_clasi cla                                                              WHERE subt.oid_clie_tipo_subt =                                                                            cla.ctsu_oid_clie_tipo_subt                                                                AND cla.clas_oid_clas IN   (%value_CLASIFICACIONVALUES%) )%end_CLASIFICACIONVALUES%               %start_TIPOCLASIFICACIONVALUES%     AND sol.clie_oid_clie IN (                                                             SELECT subt.clie_oid_clie                                                               FROM mae_clien_tipo_subti subt,                                                                    mae_clien_clasi cla                                                              WHERE subt.oid_clie_tipo_subt =                                                                       cla.ctsu_oid_clie_tipo_subt                                                                AND cla.tccl_oid_tipo_clasi IN (%value_TIPOCLASIFICACIONVALUES%)) %end_TIPOCLASIFICACIONVALUES%                           )            GROUP BY              oid_soli_cabe,             val_tipo_camb,             val_nomb_peri,             oid_peri,             cod_peri,             fec_fact            ) datos,            (            SELECT *            FROM              (              SELECT 0.1 p FROM dual UNION SELECT 0.2 FROM dual UNION SELECT 0.3 FROM dual UNION              SELECT 0.4 FROM dual UNION SELECT 0.5 FROM dual UNION SELECT 0.6 FROM dual UNION              SELECT 0.7 FROM dual UNION SELECT 0.8 FROM dual UNION SELECT 0.9 FROM dual UNION              SELECT 1  FROM dual UNION SELECT 2 FROM dual UNION 