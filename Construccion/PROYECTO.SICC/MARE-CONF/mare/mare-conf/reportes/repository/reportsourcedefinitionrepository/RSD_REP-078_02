CT    con.NUM_CONC,   con.VAL_NOMB,   COUNT(*) AS consultoras_con_metas,   (       SELECT     COUNT(*)    FROM     inc_ganad gan,     inc_param_nivel_premi niv,     inc_param_gener_premi par_pre    WHERE     gan.PANP_OID_PARA_NIVE_PREM = niv.OID_PARA_NIVE_PREM     AND niv.PAGP_OID_PARA_GENE_PREM = par_pre.OID_PARA_GENE_PREM     AND par_pre.COPA_OID_PARA_GRAL = con.OID_PARA_GRAL    ) cons_ganadoras  FROM   inc_concu_param_gener con,   (    SELECT *    FROM     (      (       SELECT *       FROM inc_metas_tipo_venta met2       WHERE met2.copa_oid_para_gral = %value_NUMEROCONCURSOVALUE%       )      MINUS       (       SELECT        met.*       FROM        inc_metas_tipo_venta met,        inc_desca des,        inc_causa_desca cau_des       WHERE        met.CLIE_OID_CLIE = des.clie_oid_clie        AND met.COPA_OID_PARA_GRAL = des.COPA_OID_PARA_GRAL        AND des.CADE_OID_CAUS_DESC = cau_des.OID_CAUS_DESC        AND cau_des.COD_CAUS in ('FA','FC')        AND met.COPA_OID_PARA_GRAL = %value_NUMEROCONCURSOVALUE%       )     )    ) cons_totales  WHERE   cons_totales.COPA_OID_PARA_GRAL = con.OID_PARA_GRAL      and con.PAIS_OID_PAIS =  %value_PAISVALUE%    and con.MARC_OID_MARC =  %value_MARCAVALUE%    and con.CANA_OID_CANA = %value_CANALVALUE%     and con.OID_PARA_GRAL = %value_NUMEROCONCURSOVALUE%   GROUP BY   con.NUM_CONC,   con.VAL_NOMB,   con.OID_PARA_GRAL  ) GROUP BY  NUM_CONC,  VAL_NOMB     " ExternalClassName="oracle.jdbc.driver.OracleDriver" ExternalSourceData="" ExternalConnectionString="" ExternalConnectionClassName="oracle.jdbc.driver.OracleDriver" ExternalConnectionLocalUrl="jdbc:oracle:thin:@192.168.118.100:1523:SICCV3P" ExternalConnectionDistributedUrl="" ExternalConnectionUser="own_peru" ExternalConnectionPassword="own_peru" DatePattern="" DateTimePattern=""/></DataSourceDefinitions><ParameterValues><ParameterValue FilterDefinitionName="PAIS" FilterParameter="VALUE" DefaultValue="1"/><ParameterValue FilterDefinitionName="MARCA" FilterParameter="VALUE" Default