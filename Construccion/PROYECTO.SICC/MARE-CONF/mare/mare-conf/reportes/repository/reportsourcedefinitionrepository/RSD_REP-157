RO_PERI_CON_PEDIDOS" Description="NRO_PERI_CON_PEDIDOS" DataType="FLOAT" Calculated="false" Expression="" ExternalData="NRO_PERI_CON_PEDIDOS"/><DimensionSourceDefinition Name="COD_TIPO_SOLI_SOL" Description="COD_TIPO_SOLI_SOL" DataType="STRING" Calculated="false" Expression="" ExternalData="COD_TIPO_SOLI_SOL"/><DimensionSourceDefinition Name="VAL_NUME_SOLI" Description="VAL_NUME_SOLI" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_NUME_SOLI"/><DimensionSourceDefinition Name="VAL_NOMB_PERI_SOL" Description="VAL_NOMB_PERI_SOL" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_NOMB_PERI_SOL"/><DimensionSourceDefinition Name="FEC_DOCU_SOL" Description="FEC_DOCU_SOL" DataType="DATETIME" Calculated="false" Expression="" ExternalData="FEC_DOCU_SOL"/><DimensionSourceDefinition Name="VAL_MONT_ORDE" Description="VAL_MONT_ORDE" DataType="FLOAT" Calculated="false" Expression="" ExternalData="VAL_MONT_ORDE"/><DimensionSourceDefinition Name="VAL_MONT_DEUD_VENC" Description="VAL_MONT_DEUD_VENC" DataType="FLOAT" Calculated="false" Expression="" ExternalData="VAL_MONT_DEUD_VENC"/><DimensionSourceDefinition Name="VAL_1" Description="VAL_1" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_1"/><DimensionSourceDefinition Name="VAL_2" Description="VAL_2" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_2"/><DimensionSourceDefinition Name="VAL_3" Description="VAL_3" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_3"/><DimensionSourceDefinition Name="VAL_4" Description="VAL_4" DataType="STRING" Calculated="false" Expression="" ExternalData="VAL_4"/><DimensionSourceDefinition Name="USUARIO" Description="USUARIO" DataType="STRING" Calculated="false" Expression="" ExternalData="USUARIO"/><DimensionSourceDefinition Name="FEC_HORA_LIBE" Description="FEC_HORA_LIBE" DataType="DATETIME" Calculated="false" Expression="" ExternalData="FEC_HORA_LIBE"/><DimensionSourceDefinition Name="ESTADO_EVAL_AUTO" Description="ESTADO_EVAL_AUTO" DataType="STRING" Calculated="false" Expression="" ExternalData="ESTADO_EVAL_AUTO"/><DimensionSourceDefinition Name="NOTIFICADO" Description="NOTIFICADO" DataType="STRING" Calculated="false" Expression="" ExternalData="NOTIFICADO"/><DimensionSourceDefinition Name="COD_NIVE_RIES_SOL" Description="COD_NIVE_RIES_SOL" DataType="STRING" Calculated="false" Expression="" ExternalData="COD_NIVE_RIES_SOL"/></DimensionSourceDefinitions><MetricSourceDefinitions><MetricSourceDefinition Name="CANTIDAD" Description="CANTIDAD" AggregateType="SUM" GroupFooterType="NONE" Calculated="false" AggregateFunction="" GroupFooterFunction="" ExternalData="CANTIDAD" ExternalCalculated="false"/></MetricSourceDefinitions><FilterDefinitions><FilterDefinition Name="IDIOMA" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="PAIS" DataType="INTEGER" FilterType="EQUALTO" Visual="false"/><FilterDefinition Name="CODIGOCLIENTE" DataType="STRING" FilterType="EQUALTO" Visual="false"/></FilterDefinitions><DataSourceDefinitions><DataSourceDefinition Name="REP-157" SourceType="SQL" Expression=" ( SELECT CLI.COD_CLIE,      (CLI.VAL_APE1 || ' ' || CLI.VAL_APE2 || ' ' || CLI.VAL_NOM1 || ' ' || CLI.VAL_NOM2) AS NOMBRE,     (      SELECT MCC.VAL_TEXT_COMU      FROM MAE_CLIEN_COMUN MCC             WHERE MCC.CLIE_OID_CLIE = CLI.OID_CLIE         AND MCC.TICM_OID_TIPO_COMU = 1      ) TELEFONO,     (         SELECT GEN_ESTA.VAL_I18N       FROM MAE_CLIEN_DATOS_ADICI CDA,        MAE_ESTAT_CLIEN MEC,     (       SELECT GEN.VAL_OID, GEN.VAL_I18N      FROM V_GEN_I18N_SICC GEN      WHERE GEN.ATTR_ENTI = 'MAE_ESTAT_CLIEN'         AND GEN.IDIO_OID_IDIO = %value_IDIOMAVALUE%     ) GEN_ESTA          WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE         AND CDA.ESTA_OID_ESTA_CLIE = MEC.OID_ESTA_CLIE      AND MEC.OID_ESTA_CLIE = GEN_ESTA.VAL_OID(+)        AND CDA.IND_ACTI = 1     ) COD_ESTA_CLIE,     (      SELECT MCC.VAL_TEXT_COMU      FROM MAE_CLIEN_COMUN MCC             WHERE MCC.CLIE_OID_CLIE = CLI.OID_CLIE         AND MCC.TICM_OID_TIPO_COMU = 3      ) E_MAIL,     (         SELECT GEN_CAN.VAL_I18N      FROM MAE_CLIEN_UNIDA_ADMIN CLI_UA,        SEG_CANAL CANAL,     ZON_TERRI_ADMIN ZTA,     (       SELECT GEN.VAL_OID, GEN.VAL_I18N      FROM V_GEN_I18N_SICC GEN      WHERE GEN.ATTR_ENTI = 'SEG_CANAL'         AND GEN.IDIO_OID_IDIO = %value_IDIOMAVALUE%     ) GEN_CAN      WHERE CLI_UA.CLIE_OID_CLIE = CLI.OID_CLIE         AND CLI_UA.IND_ACTI = 1      AND CLI_UA.ZTAD_OID_TERR_ADMI = ZTA.OID_TERR_ADMI      AND CANAL.OID_CANA = ZTA.CANA_OID_CANA      AND CANAL.OID_CANA = GEN_CAN.VAL_OID(+)          ) COD_CANA,     (         SELECT MARCA.DES_MARC      FROM MAE_CLIEN_UNIDA_ADMIN CLI_UA,        SEG_MARCA MARCA,     ZON_TERRI_ADMIN ZTA      WHERE CLI_UA.CLIE_OID_CLIE = CLI.OID_CLIE         AND CLI_UA.IND_ACTI = 1      AND CLI_UA.ZTAD_OID_TERR_ADMI = ZTA.OID_TERR_ADMI      AND MARCA.OID_MARC = ZTA.MARC_OID_MARC     ) COD_MARC,     (         SELECT PER.VAL_NOMB_PERI      FROM MAE_CLIEN_HISTO_ESTAT CHE,        CRA_PERIO PER      WHERE CHE.ESTA_OID_ESTA_CLIE = 1         AND CHE.CLIE_OID_CLIE = CLI.OID_CLIE      AND PER.OID_PERI = CHE.PERD_OID_PERI      ) VAL_NOMB_PERI,     CASE WHEN ( SELECT T.COD_TIPO_BLOQ        FROM MAE_CLIEN_BLOQU CB,             MAE_TIPO_BLOQU T        WHERE CB.CLIE_OID_CLIE = CLI.OID_CLIE           AND CB.TIBQ_OID_TIPO_BLOQ = T.OID_TIPO_BLOQ        AND T.COD_TIPO_BLOQ = '02') = '02' THEN     'SI'     ELSE     'NO'     END BLOQ_ADMIN,     (          SELECT CB.VAL_MOTI_BLOQ     FROM MAE_CLIEN_BLOQU CB,         MAE_TIPO_BLOQU T    WHERE CB.CLIE_OID_CLIE = CLI.OID_CLIE                  AND CB.TIBQ_OID_TIPO_BLOQ = T.OID_TIPO_BLOQ      AND T.COD_TIPO_BLOQ = '02'     ) MOTIV_BLOQ,     CLI.FEC_INGR,     CASE WHEN ( SELECT T.COD_TIPO_BLOQ          FROM MAE_CLIEN_BLOQU CB,             MAE_TIPO_BLOQU T        WHERE CB.CLIE_OID_CLIE = CLI.OID_CLIE           AND CB.TIBQ_OID_TIPO_BLOQ = T.OID_TIPO_BLOQ        AND T.COD_TIPO_BLOQ = '01') = '01' THEN     'SI'   ELSE     'NO'   END BLOQ_FINAN,        (        SELECT COD_TIPO_SOLI     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) COD_TIPO_SOLI,     (        SELECT NUM_IDEN_CUOT     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) NUM_IDEN_CUOT,     (        SELECT VAL_DENO     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) VAL_DENO,     (        SELECT FEC_DOCU     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) FEC_DOCU,     (        SELECT FEC_VENC     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) FEC_VENC,        (        SELECT COD_FORM_PAGO     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) COD_FORM_PAGO,     (        SELECT IMP_MOVI     FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) MONTO,     (        SELECT (IMP_MOVI - IMP_PAGA)      FROM     (         SELECT SOCI.COD_SOCI, SOCI.VAL_DENO, M.*, FPAGO.COD_FORM_PAGO, TSOLIC.COD_TIPO_SOLI     FROM   CCC_MOVIM_CUENT_CORRI M,               CCC_SUBPR S,            CCC_PROCE P,             SEG_SOCIE SOCI,            BEL_FORMA_PAGO FPAGO,             PED_TIPO_SOLIC_PAIS TSOLIC_PA,            PED_TIPO_SOLIC TSOLIC,            PED_SOLIC_CABEC SOLIC_CAB,( SELECT OID_CLIE FROM MAE_CLIEN WHERE COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND PAIS_OID_PAIS = %value_PAISVALUE% ) CLIENTE                  WHERE  M.SUBP_OID_SUBP_CREA = S.OID_SUBP          AND S.CCPR_OID_PROC = P.OID_PROC         AND M.SUBP_OID_SUBP_CREA = 1          AND P.COD_PROC = 'CCC001'         AND M.TSPA_OID_TIPO_SOLI_PAIS(+) = TSOLIC_PA.OID_TIPO_SOLI_PAIS         AND M.SOCI_OID_SOCI = SOCI.OID_SOCI(+)      AND SOLIC_CAB.TSPA_OID_TIPO_SOLI_PAIS = TSOLIC_PA.OID_TIPO_SOLI_PAIS       AND SOLIC_CAB.FOPA_OID_FORM_PAGO = FPAGO.OID_FORM_PAGO(+)          AND TSOLIC_PA.TSOL_OID_TIPO_SOLI = TSOLIC.OID_TIPO_SOLI(+)      AND TSOLIC_PA.PAIS_OID_PAIS = FPAGO.PAIS_OID_PAIS      AND TSOLIC_PA.PAIS_OID_PAIS = %value_PAISVALUE%      AND M.CLIE_OID_CLIE = CLIENTE.OID_CLIE      AND (NVL(M.IMP_MOVI,0) - NVL(M.IMP_PAGA,0)) &lt;> 0     ORDER BY M.FEC_DOCU  DESC     )   WHERE ROWNUM = 1     ) MONTO_SALDO,     PQ_APL_REP.FN_CU_CALC_SALDO_CTA_CTE(CLI.OID_CLIE) AS SALDO_ACTUAL,      NVL((    SELECT COUNT(DISTINCT m.NUM_IDEN_CUOT) AS CANT    FROM   CCC_CABEC_DTO_RECAR_FINAN CAB,      CCC_DETAL_DTO_RECAR_FINAN DET,      CCC_MOVIM_CUENT_CORRI M,     CCC_SUBPR S,     CCC_PROCE P     WHERE  CAB.SUBP_OID_SUBP = S.OID_SUBP     AND S.CCPR_OID_PROC = P.OID_PROC    AND S.COD_SUBP = 1     AND P.COD_PROC = 'CCC005'    AND CAB.FEC_DOCU>=(ADD_MONTHS(SYSDATE,-6))    AND CAB.PAIS_OID_PAIS = %value_PAISVALUE%    AND DET.CDRF_OID_CABE_DTO_RECA_FINA = CAB.OID_CABE_DTO_RECA_FINA    AND DET.MVCC_OID_MOVI_CC = M.OID_MOVI_CC    AND CAB.CLIE_OID_CLIE = CLI.OID_CLIE    GROUP BY CAB.CLIE_OID_CLIE, M.NUM_IDEN_CUOT, M.NUM_ORDE_CUOT     ),0) AS N_VECES_PRONTO_PAGO,         NVL((    SELECT COUNT(DISTINCT m.NUM_IDEN_CUOT) AS CANT    FROM   CCC_CABEC_DTO_RECAR_FINAN CAB,      CCC_DETAL_DTO_RECAR_FINAN DET,      CCC_MOVIM_CUENT_CORRI M,     CCC_SUBPR S,     CCC_PROCE P     WHERE  CAB.SUBP_OID_SUBP = S.OID_SUBP     AND S.CCPR_OID_PROC = P.OID_PROC    AND S.COD_SUBP = 2     AND P.COD_PROC = 'CCC005'    AND CAB.FEC_DOCU>=(ADD_MONTHS(SYSDATE,-6))    AND CAB.PAIS_OID_PAIS = %value_PAISVALUE%    AND DET.CDRF_OID_CABE_DTO_RECA_FINA = CAB.OID_CABE_DTO_RECA_FINA    AND DET.MVCC_OID_MOVI_CC = M.OID_MOVI_CC    AND CAB.CLIE_OID_CLIE = CLI.OID_CLIE    GROUP BY CAB.CLIE_OID_CLIE, M.NUM_IDEN_CUOT, M.NUM_ORDE_CUOT     ),0) AS N_VECES_REC_FINANC,     PQ_APL_REP.FN_CU_CALC_DIAS_ATRASO_CLIEN(CLI.PAIS_OID_PAIS, CLI.OID_CLIE) AS PROM_DIAS_MORA,     PQ_APL_REP.FN_CU_CALC_DEUDA_VENC_CLIEN(CLI.PAIS_OID_PAIS, CLI.OID_CLIE) AS VAL_TOT_MORA,     DECODE(PQ_APL_REP.FN_CU_CALC_SALDO_CTA_CTE(CLI.OID_CLIE),0,0,(PQ_APL_REP.FN_CU_CALC_DEUDA_VENC_CLIEN(CLI.PAIS_OID_PAIS,CLI.OID_CLIE)/PQ_APL_REP.FN_CU_CALC_SALDO_CTA_CTE(CLI.OID_CLIE))*100) AS PORC_MORA_RESP_FACT,     (      CASE WHEN(          SELECT NVL(SUM(ASIGN.IMP_DEUD_ASIG) - SUM(ASIGN.IMP_DEUD_CANC),0)       FROM COB_ASIGN_COBRA ASIGN,          CCC_MOVIM_CUENT_CORRI MOVI       WHERE ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC           AND MOVI.CLIE_OID_CLIE = CLI.OID_CLIE          )>0 THEN 'SI' ELSE 'NO'   END     ) AS EN_COBRANZA,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,          COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,         CCC_MOVIM_CUENT_CORRI MOVI,         COB_CRONO_COBRA CRONO_COBRA,         COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=1     ) AS ETAPA_1,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,          COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,        CCC_MOVIM_CUENT_CORRI MOVI,         COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE      WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=1     ) AS N_VECES_ETAPA_1,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,       CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,        MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=2     ) AS ETAPA_2,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,       COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,                           CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,         COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD       AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=2     ) AS N_VECES_ETAPA_2,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES          FROM COB_ASIGN_COBRA ASIGN,                  CCC_MOVIM_CUENT_CORRI MOVI,          COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC       AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD                          AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=3     ) AS ETAPA_3,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,       COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,           CCC_MOVIM_CUENT_CORRI MOVI,           COB_CRONO_COBRA CRONO_COBRA,            COB_ETAPA_DEUDA ETAPA,            MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR           AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC            AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD            AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE           AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%            AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=3     ) AS N_VECES_ETAPA_3,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,         CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE      WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD         AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE        AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%         AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=4     ) AS ETAPA_4,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,            CCC_MOVIM_CUENT_CORRI MOVI,         COB_CRONO_COBRA CRONO_COBRA,         COB_ETAPA_DEUDA ETAPA,          MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=4     ) AS N_VECES_ETAPA_4,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,          COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,          CCC_MOVIM_CUENT_CORRI MOVI,          COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=5     ) AS ETAPA_5,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,                           CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,         COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%           AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD      ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=5     ) AS N_VECES_ETAPA_5,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,        CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,        MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD       AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_