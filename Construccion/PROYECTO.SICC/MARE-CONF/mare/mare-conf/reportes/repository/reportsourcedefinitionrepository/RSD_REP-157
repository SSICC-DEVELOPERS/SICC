    (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,          COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,        CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,        MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=7     ) AS N_VECES_ETAPA_7,     (   SELECT CODIGO    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,        CCC_MOVIM_CUENT_CORRI MOVI,         COB_CRONO_COBRA CRONO_COBRA,           COB_ETAPA_DEUDA ETAPA,        MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC        AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=8     ) AS ETAPA_8,     (    SELECT VECES    FROM         (    SELECT         ROWNUM AS R, DATOS.CODIGO, DATOS.VECES         FROM         (         SELECT ETAPA.COD_ETAP_DEUD AS CODIGO,           COUNT(ETAPA.OID_ETAP_DEUD) AS  VECES         FROM COB_ASIGN_COBRA ASIGN,         CCC_MOVIM_CUENT_CORRI MOVI,        COB_CRONO_COBRA CRONO_COBRA,        COB_ETAPA_DEUDA ETAPA,         MAE_CLIEN MCLIE         WHERE ASIGN.CRCO_OID_CRON_COBR = CRONO_COBRA.OID_CRON_COBR        AND ASIGN.MVCC_OID_MOVI_CC = MOVI.OID_MOVI_CC       AND CRONO_COBRA.ETDE_OID_ETAP_DEUD = ETAPA.OID_ETAP_DEUD        AND MCLIE.OID_CLIE = MOVI.CLIE_OID_CLIE       AND MCLIE.PAIS_OID_PAIS = %value_PAISVALUE%        AND MCLIE.COD_CLIE = %value_CODIGOCLIENTEVALUE%       GROUP BY ETAPA.COD_ETAP_DEUD       ORDER BY ETAPA.COD_ETAP_DEUD      ) DATOS    WHERE       ROWNUM&lt;=8    )   WHERE R=8     ) AS N_VECES_ETAPA_8,     (      SELECT USU.VAL_APEL_PATE || ' ' || USU.VAL_APE_MATE || ' ' || USU.VAL_NOM1 || ' ' || USU.VAL_NOM2   FROM CAR_ASIGN_EJECU_CUENT AS_EJ,     CAR_EJECU_CUENT EJ_CTA,     V_REP_USUAR_LARGO USU,     ZON_ZONA ZONA,     ZON_REGIO REGION,     ZON_TERRI_ADMIN TERR_ADMIN,     ZON_SECCI SECCION,     MAE_CLIEN_UNIDA_ADMIN C_UN,     MAE_CLIEN_DATOS_ADICI CDA      WHERE      TERR_ADMIN.OID_TERR_ADMI = C_UN.ZTAD_OID_TERR_ADMI     AND C_UN.CLIE_OID_CLIE = CLI.OID_CLIE      AND C_UN.IND_ACTI = 1     AND TERR_ADMIN.ZSCC_OID_SECC = SECCION.OID_SECC     AND SECCION.ZZON_OID_ZONA = ZONA.OID_ZONA     AND ZONA.ZORG_OID_REGI = REGION.OID_REGI     AND AS_EJ.ZZON_OID_ZONA = ZONA.OID_ZONA     AND AS_EJ.ZORG_OID_REGI = REGION.OID_REGI     AND AS_EJ.ZSGV_OID_SUBG_VENT = REGION.ZSGV_OID_SUBG_VENT     AND AS_EJ.EJCU_OID_EJEC_CUEN = EJ_CTA.OID_EJEC_CUEN     AND EJ_CTA.USER_EJEC_CUEN = USU.COD_USUA     AND CDA.CLIE_OID_CLIE = C_UN.CLIE_OID_CLIE     AND CDA.NIRI_OID_NIVE_RIES = AS_EJ.NIRI_OID_NIVE_RIES     AND CDA.IND_ACTI = 1     ) AS RESP_CRED,     NVL((       SELECT CDA.IMP_MONT_LINE_CRED    FROM MAE_CLIEN_DATOS_ADICI CDA    WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE        AND CDA.IND_ACTI = 1     ), 0) LIN_CRED_ACTUAL,         NVL(((SELECT CDA.IMP_MONT_LINE_CRED          FROM MAE_CLIEN_DATOS_ADICI CDA       WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE       AND CDA.IND_ACTI = 1) -       PQ_APL_REP.FN_CU_CALC_MONTO_SOLIC_NUEVAS(CLI.OID_CLIE) -       PQ_APL_REP.FN_CU_CALC_SALDO_CTA_CTE(CLI.OID_CLIE) +      PQ_APL_REP.FN_CU_CALC_CUPON_EN_TRAMITE(CLI.PAIS_OID_PAIS, CLI.OID_CLIE)     ),0) AS LIN_CRED_DISP,     (      SELECT PER.VAL_NOMB_PERI   FROM CRA_PERIO PER,     MAE_CLIEN_DATOS_ADICI CDA   WHERE PER.OID_PERI = CDA.PERD_OID_PERI_LINE_CRED      AND CDA.CLIE_OID_CLIE = CLI.OID_CLIE      AND CDA.IND_ACTI = 1     ) AS PERI_ULT_EVAL_CRED,    PQ_APL_REP.FN_CU_CALC_PED_PROMEDIO_CLIEN(CLI.OID_CLIE) AS PED_PROM_ULT_EVAL,     DECODE(         NVL((         (       SELECT CDA.IMP_MONT_LINE_CRED    FROM MAE_CLIEN_DATOS_ADICI CDA    WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE        AND CDA.IND_ACTI = 1       ) -          pq_apl_rep.FN_CU_CALC_MONTO_SOLIC_NUEVAS(CLI.OID_CLIE) -          pq_apl_rep.FN_CU_CALC_SALDO_CTA_CTE (CLI.OID_CLIE) +         pq_apl_rep.FN_CU_CALC_CUPON_EN_TRAMITE (CLI.PAIS_OID_PAIS, CLI.OID_CLIE)         ),0),      0,      0,         NVL((       SELECT CDA.IMP_MONT_LINE_CRED    FROM MAE_CLIEN_DATOS_ADICI CDA    WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE        AND CDA.IND_ACTI = 1),0) /      NVL((         (       SELECT CDA.IMP_MONT_LINE_CRED    FROM MAE_CLIEN_DATOS_ADICI CDA    WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE        AND CDA.IND_ACTI = 1       ) -          pq_apl_rep.FN_CU_CALC_MONTO_SOLIC_NUEVAS(CLI.OID_CLIE) -          pq_apl_rep.FN_CU_CALC_SALDO_CTA_CTE (CLI.OID_CLIE) +         pq_apl_rep.FN_CU_CALC_CUPON_EN_TRAMITE (CLI.PAIS_OID_PAIS, CLI.OID_CLIE)         ),0)         ) AS PORC_LIN_CRED_ACT_CONS,    (     SELECT COD_NIVE_RIES     FROM MAE_CLIEN_DATOS_ADICI CDA,         CAR_NIVEL_RIESG CNR     WHERE CDA.CLIE_OID_CLIE = CLI.OID_CLIE       AND CDA.NIRI_OID_NIVE_RIES = CNR.OID_NIVE_RIES      AND CDA.IND_ACTI = 1     ) COD_NIVE_RIES,    (      SELECT PER.VAL_NOMB_PERI   FROM CRA_PERIO PER,     MAE_CLIEN_DATOS_ADICI CDA     WHERE PER.OID_PERI = CDA.PERD_OID_PERI_NIVE_RIES      AND CDA.CLIE_OID_CLIE = CLI.OID_CLIE      AND CDA.IND_ACTI = 1       ) AS PERI_ULT_EVAL_RIES,  PQ_APL_REP.FN_CU_CALC_PERI_CON_PEDIDOS(CLI.OID_CLIE) AS NRO_PERI_CON_PEDIDOS,        NULL AS COD_TIPO_SOLI_SOL,        NULL AS VAL_NUME_SOLI,        NULL AS VAL_NOMB_PERI_SOL,        NULL AS FEC_DOCU_SOL,        NULL AS VAL_MONT_ORDE,      NULL AS VAL_MONT_DEUD_VENC,        NULL AS VAL_1,      NULL AS VAL_2,      NULL AS VAL_3,        NULL AS VAL_4,      NULL AS USUARIO,        NULL AS FEC_HORA_LIBE,      NULL AS ESTADO_EVAL_AUTO,        NULL AS NOTIFICADO,        NULL AS COD_NIVE_RIES_SOL,     0 AS CANTIDAD  FROM MAE_CLIEN CLI  WHERE CLI.COD_CLIE = %value_CODIGOCLIENTEVALUE%    AND CLI.PAIS_OID_PAIS = %value_PAISVALUE% ) UNION ALL (    SELECT %value_CODIGOCLIENTEVALUE% AS COD_CLIE,       NULL AS NOMBRE,            NULL AS TELEFONO,      NULL AS COD_ESTA_CLIE,      NULL AS E_MAIL,      NULL AS COD_CANA,      NULL AS COD_MARC,      NULL AS VAL_NOMB_PERI,      NULL AS BLOQ_ADMIN,      NULL AS MOTIV_BLOQ,      NULL AS FEC_INGR,      NULL AS BLOQ_FINAN,         NULL AS COD_TIPO_SOLI,      NULL AS NUM_IDEN_CUOT,      NULL AS VAL_DENO,      NULL AS FEC_DOCU,      NULL AS FEC_VENC,         NULL AS COD_FORM_PAGO,      NULL AS MONTO,      NULL AS MONTO_SALDO,      NULL AS SALDO_ACTUAL,         NULL AS N_VECES_PRONTO_PAGO,      NULL AS N_VECES_REC_FINANC,      NULL AS PROM_DIAS_MORA,      NULL AS VAL_TOT_MORA,      NULL AS PORC_MORA_RESP_FACT,          NULL AS EN_COBRANZA,      NULL AS ETAPA_1,      NULL AS N_VECES_ETAPA_1,      NULL AS ETAPA_2,      NULL AS N_VECES_ETAPA_2,      NULL AS ETAPA_3,      NULL AS N_VECES_ETAPA_3,      NULL AS ETAPA_4,         NULL AS N_VECES_ETAPA_4,      NULL AS ETAPA_5,         NULL AS N_VECES_ETAPA_5,      NULL AS ETAPA_6,         NULL AS N_VECES_ETAPA_6,        NULL AS ETAPA_7,        NULL AS N_VECES_ETAPA_7,         NULL AS ETAPA_8,      NULL AS N_VECES_ETAPA_8,      NULL AS RESP_CRED,      NULL AS LIN_CRED_ACTUAL,          NULL AS LIN_CRED_DISP,        NULL AS PERI_ULT_EVAL_CRED,      NULL AS PED_PROM_ULT_EVAL,      NULL AS PORC_LIN_CRED_ACT_CONS,      NULL AS COD_NIVE_RIES,      