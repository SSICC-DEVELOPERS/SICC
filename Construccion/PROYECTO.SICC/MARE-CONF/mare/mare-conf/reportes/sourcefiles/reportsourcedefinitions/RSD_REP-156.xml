	<DataSourceDefinitions>  
		<DataSourceDefinition Name="REP-156" SourceType="SQL" Expression="  
SELECT PAIS.COD_PAIS,  
              SGV_ACTUAL.COD_SUBG_VENT,   
              REG_ACTUAL.COD_REGI,   
                             ZON_ACTUAL.COD_ZONA,   
                             SEC_ACTUAL.COD_SECC,   
                             TERR_ACTUAL.COD_TERR,   
                             SEC_ACTUAL.DES_SECCI  SECCION,  
                             CLI_ZONA.VAL_NOM1||' '||CLI_ZONA.VAL_NOM2||' '||CLI_ZONA.VAL_APE1||' '||CLI_ZONA.VAL_APE2 GERENTE_ZONA,  
                             CLI_SECC.VAL_NOM1||' '||CLI_SECC.VAL_NOM2||' '||CLI_SECC.VAL_APE1||' '||CLI_SECC.VAL_APE2 GERENTE_SECC,  
                             CLI_UNI.COD_CLIE,  
                             CLI_UNI.VAL_NOM1||' '||CLI_UNI.VAL_NOM2||' '||CLI_UNI.VAL_APE1||' '||CLI_UNI.VAL_APE2 NOMBRE_COMPLETO,  
                             PAIS_ANTE.COD_PAIS COD_PAIS_1 ,   
                             SGV_ANTE.COD_SUBG_VENT COD_SUBG_VENT_1,   
                             REG_ANTE.COD_REGI COD_REGI_1,   
                             ZON_ANTE.COD_ZONA COD_ZONA_1,   
                             SEC_ANTE.COD_SECC COD_SECC_1,   
                             TERR_ANTE.COD_TERR COD_TERR_1,   
                             0 AS CANTIDAD                                               
FROM  
(  
                    SELECT CLIEN.CLIE_OID_CLIE,  
                                                 CLIEN.PERD_OID_PERI_INI OID_PERI_INI_ACTUAL,  
                                                CLIEN.PERD_OID_PERI_FIN OID_PERI_FIN_ACTUAL,  
                                                 CLIEN.ZTAD_OID_TERR_ADMI OID_TERR_ADMI_ACTUAL,  
                                                 MAX(CUA.PERD_OID_PERI_INI) OID_PERI_INI_ANTE,  
                                                 MAX(CUA.PERD_OID_PERI_FIN) OID_PERI_FIN_ANTE,  
                                                 MAX(CUA.ZTAD_OID_TERR_ADMI) OID_TERR_ADMI_ANTE  
                    FROM  
                    (  
                                               SELECT CUA.CLIE_OID_CLIE,  
                                                                          CUA.PERD_OID_PERI_INI,  
                                                                          CUA.PERD_OID_PERI_FIN,  
                                                                          CUA.ZTAD_OID_TERR_ADMI,                                                                                                                                
                                                                          MIN( CUA.OID_CLIE_UNID_ADMI) OID_CLIE_UNID_ADMI  
                                               FROM (  
                                                                                SELECT P.FEC_INIC  
                                                                                FROM CRA_PERIO P   
                                                                                WHERE P.OID_PERI =%value_PERIODODESDEVALUE%  
   
                                                                    ) PER_INI,  
                                                                     (  
                                                                             SELECT P.FEC_FINA  
                                                                            FROM CRA_PERIO P  
                                                                            WHERE P.OID_PERI = DECODE(%value_PERIODOHASTAVALUE%,-1,%value_PERIODODESDEVALUE%,%value_PERIODOHASTAVALUE%)                              
                                                                     ) PER_FIN,  
                                                                     MAE_CLIEN_UNIDA_ADMIN CUA,  
                                                                     CRA_PERIO PER,  
                                                                     ZON_SUB_GEREN_VENTA SGV,  
                                                                     ZON_REGIO REG,  
                                                                     ZON_ZONA ZON,  
                                                                     ZON_SECCI SEC,  
                                                                     ZON_TERRI_ADMIN ZTA,  
                                                                     MAE_CLIEN CLI  
                                               WHERE PER.PAIS_OID_PAIS = %value_PAISVALUE%  
                                                                         AND PER.MARC_OID_MARC = %value_MARCAVALUE%  
                                                                         AND PER.CANA_OID_CANA = %value_CANALVALUE%  
                                                                         AND PER.FEC_INIC >= PER_INI.FEC_INIC  
                                                                         AND PER.FEC_FINA &lt;= PER_FIN.FEC_FINA  
                                                                         AND CUA.PERD_OID_PERI_INI = PER.OID_PERI  
  %start_CODIGOCLIENTEVALUE% AND CLI.COD_CLIE = %value_CODIGOCLIENTEVALUE% %end_CODIGOCLIENTEVALUE%  
  %start_ZONAVALUE% AND ZON.OID_ZONA = %value_ZONAVALUE% %end_ZONAVALUE%   
  %start_REGIONVALUE% AND REG.OID_REGI = %value_REGIONVALUE% %end_REGIONVALUE%  
  %start_SUBGERENCIAVENTAVALUE% AND SGV.OID_SUBG_VENT = %value_SUBGERENCIAVENTAVALUE% %end_SUBGERENCIAVENTAVALUE%  
                                                                         AND CUA.CLIE_OID_CLIE =  CLI.OID_CLIE  
                                                                         AND SGV.OID_SUBG_VENT = REG.ZSGV_OID_SUBG_VENT  
                                                                         AND REG.OID_REGI = ZON.ZORG_OID_REGI  
                                                                         AND ZON.OID_ZONA = SEC.ZZON_OID_ZONA  
                                                                         AND SEC.OID_SECC = ZTA.ZSCC_OID_SECC  
                                                                          AND CUA.ZTAD_OID_TERR_ADMI = ZTA.OID_TERR_ADMI          
                                               GROUP BY CUA.CLIE_OID_CLIE,  
                                                                                        CUA.PERD_OID_PERI_INI,  
                                                                                        CUA.PERD_OID_PERI_FIN,  
                                                                                        CUA.ZTAD_OID_TERR_ADMI    
                    ) CLIEN,  
                    MAE_CLIEN_UNIDA_ADMIN CUA,  
                    CRA_PERIO PER_CLIEN,  
                    CRA_PERIO PER_CUA  
                    WHERE PER_CUA.OID_PERI = CUA.PERD_OID_PERI_FIN  
                                                 AND PER_CLIEN.OID_PERI = CLIEN.PERD_OID_PERI_INI  
                                                 AND PER_CLIEN.FEC_FINA >= PER_CUA.FEC_FINA  
                                                 AND CLIEN.CLIE_OID_CLIE = CUA.CLIE_OID_CLIE  
                                                 AND CUA.OID_CLIE_UNID_ADMI &lt;> CLIEN.OID_CLIE_UNID_ADMI     
                    GROUP BY CLIEN.CLIE_OID_CLIE,  
                                                 CLIEN.PERD_OID_PERI_INI,  
                                                CLIEN.PERD_OID_PERI_FIN,  
                                                 CLIEN.ZTAD_OID_TERR_ADMI  
) DATOS,  
MAE_CLIEN CLI_UNI,  
MAE_CLIEN CLI_ZONA,  
MAE_CLIEN CLI_SECC,  
ZON_SUB_GEREN_VENTA SGV_ACTUAL,  
ZON_REGIO REG_ACTUAL,  
ZON_ZONA ZON_ACTUAL,  
ZON_SECCI SEC_ACTUAL,  
ZON_TERRI_ADMIN ZTA_ACTUAL,  
ZON_TERRI TERR_ACTUAL,  
ZON_SUB_GEREN_VENTA SGV_ANTE,  
ZON_REGIO REG_ANTE,  
ZON_ZONA ZON_ANTE,  
ZON_SECCI SEC_ANTE,  
ZON_TERRI_ADMIN ZTA_ANTE,  
ZON_TERRI TERR_ANTE,  
SEG_PAIS PAIS_ANTE,  
SEG_PAIS PAIS  
WHERE DATOS.CLIE_OID_CLIE = CLI_UNI.OID_CLIE  
                             AND DATOS.OID_TERR_ADMI_ACTUAL = ZTA_ACTUAL.OID_TERR_ADMI  
                             AND SGV_ACTUAL.OID_SUBG_VENT = REG_ACTUAL.ZSGV_OID_SUBG_VENT  
                            AND REG_ACTUAL.OID_REGI = ZON_ACTUAL.ZORG_OID_REGI  
                      AND ZON_ACTUAL.OID_ZONA = SEC_ACTUAL.ZZON_OID_ZONA  
                             AND SEC_ACTUAL.OID_SECC = ZTA_ACTUAL.ZSCC_OID_SECC  
                             AND TERR_ACTUAL.OID_TERR = ZTA_ACTUAL.TERR_OID_TERR  
                             AND ZON_ACTUAL.CLIE_OID_CLIE = CLI_ZONA.OID_CLIE(+)  
                             AND SEC_ACTUAL.CLIE_OID_CLIE = CLI_SECC.OID_CLIE(+)            
                             AND DATOS.OID_TERR_ADMI_ANTE = ZTA_ANTE.OID_TERR_ADMI  
                             AND SGV_ANTE.OID_SUBG_VENT = REG_ANTE.ZSGV_OID_SUBG_VENT  
                             AND REG_ANTE.OID_REGI = ZON_ANTE.ZORG_OID_REGI  
                      AND ZON_ANTE.OID_ZONA = SEC_ANTE.ZZON_OID_ZONA  
                             AND SEC_ANTE.OID_SECC = ZTA_ANTE.ZSCC_OID_SECC  
                             AND TERR_ANTE.OID_TERR = ZTA_ANTE.TERR_OID_TERR  
                             AND SGV_ANTE.PAIS_OID_PAIS = PAIS_ANTE.OID_PAIS  
                             AND PAIS.OID_PAIS = SGV_ACTUAL.PAIS_OID_PAIS  
		" ExternalClassName="oracle.jdbc.driver.OracleDriver" ExternalSourceData="" ExternalConnectionString="" ExternalConnectionClassName="oracle.jdbc.driver.OracleDriver" ExternalConnectionLocalUrl="jdbc:oracle:thin:@192.168.118.106:1523:SICCV3DG" ExternalConnectionDistributedUrl="" ExternalConnectionUser="usu_peru" ExternalConnectionPassword="usu_peru" DatePattern="" DateTimePattern=""/>  
	</DataSourceDefinitions>  
