<?xml version="1.0" encoding="windows-1252" ?>
<mare>
    <default>
        <principal>oc4jadmin</principal>
        <credential>welcome</credential>
        <provider>ormi://localhost:23891/current-workspace-app</provider>
        <!-- Remoto 
        <contextFactory>oracle.j2ee.rmi.RMIInitialContextFactory</contextFactory>
		-->
        <!-- Local -->
        <contextFactory>com.evermind.server.ApplicationInitialContextFactory</contextFactory>
    </default>
    <services>
        <EJBLocator>
            <class>es.indra.mare.common.mii.services.ejbservice.EJBLocator</class>
            <principal>${default.principal}</principal>
            <credential>${default.credential}</credential>
            <provider>${default.provider}</provider>
            <contextFactory>${default.contextFactory}</contextFactory>
        </EJBLocator>
        <JMSLocator>
            <class>es.indra.mare.common.mii.services.jms.JMSLocator</class>
            <principal>${default.principal}</principal>
            <credential>${default.credential}</credential>
            <provider>${default.provider}</provider>
            <contextFactory>${default.contextFactory}</contextFactory>
        </JMSLocator>
        <Configuration>
            <class>es.indra.mare.common.mgc.MareMGC</class>
            <prefijos>
                <MiiServices>services</MiiServices>
                <ApplicationValues>application</ApplicationValues>
                <MonMapping>monMapping</MonMapping>
                <Directorios>directorios</Directorios>
            </prefijos>
            <directorios>
                <businessDefinition>business/definition/</businessDefinition>
                <businessMapping>business/monMapping/</businessMapping>        
            </directorios>
        </Configuration>
        <EngineService>
            <class>es.indra.mare.common.mln.engine.EngineManager</class>
        </EngineService>
        <EngineFactoryService>
            <class>es.indra.mare.common.mln.engine.EngineFactory</class>
            <rootPath>${sys:MareConfigurationRoot}</rootPath>
            <addfile>${sys:MareConfigurationRoot}/services/engines.properties</addfile>
        </EngineFactoryService>
        <ActivatorService>
            <class>es.indra.mare.common.activator.ActivatorFactory</class>
            <rootPath>${sys:MareConfigurationRoot}</rootPath>
            <addfile>${sys:MareConfigurationRoot}/services/activators.properties</addfile>
        </ActivatorService>
        <LoggingService>
            <class>es.indra.mare.common.mii.services.logging.LoggingService</class>
            <delay>5000</delay>
            <Log4JConfigurationFile>services/log4j.lcf</Log4JConfigurationFile>
        </LoggingService>
        <APPLoggingService>
            <class>${services.LoggingService.class}</class>
            <delay>${services.LoggingService.delay}</delay>
            <Log4JConfigurationFile>services/APPLoggingService.lcf</Log4JConfigurationFile>            
        </APPLoggingService>
        <ScriptingService>
            <class>es.indra.mare.common.mii.services.scripting.ScriptingService</class>
        </ScriptingService>
        <VFSService>
            <class>es.indra.mare.common.mii.services.vfs.MareVFS</class>
        </VFSService>
        <ValidationService>
            <class>es.indra.mare.common.mii.services.validation.ValidationService</class>
            <userCanSpecifyValidations>true</userCanSpecifyValidations>
            <testValidation>${MareConfigurationRoot}/services/validationservice/validator-example.xml</testValidation>
        </ValidationService>
        <JDBCService>
            <class>es.indra.mare.common.mii.services.jdbcservice.DBService</class>
            <principal>${default.principal}</principal>
            <credential>${default.credential}</credential>
            <provider>${default.provider}</provider>
            <contextFactory>${default.contextFactory}</contextFactory>
            <datasource>jdbc/USU_PERUDS</datasource>
            <db>
                <pool>true</pool>
                <url>jdbc:oracle:thin:@192.168.69.2:1523:SICCDES4</url>
                <driver>oracle.jdbc.driver.OracleDriver</driver>
                <user>usu_peru</user>
                <password>usu_peru</password>
            </db>        
        </JDBCService>
        <CalipsoService>
            <class>es.indra.sicc.rep.service.CalipsoService</class>
            <principal>${default.principal}</principal>
            <credential>${default.credential}</credential>
            <!--provider>${default.provider}</provider-->
            <url>${default.provider}</url>
            <!--contextFactory>${default.contextFactory}</contextFactory-->
            <initial>${default.contextFactory}</initial>
            <datasource>${services.JDBCService.datasource}</datasource>
            <db>
                <pool>${services.JDBCService.db.pool}</pool>
                <url>${services.JDBCService.db.url}</url>
                <driver>${services.JDBCService.db.driver}</driver>
                <user>${services.JDBCService.db.user}</user>
                <password>${services.JDBCService.db.password}</password>
            </db>
        </CalipsoService>
        <MGUService>
            <class>es.indra.mare.common.mgu.MGUService</class>
            <principal>${default.principal}</principal>
            <credential>${default.credential}</credential>
            <provider>${default.provider}</provider>
            <contextFactory>${default.contextFactory}</contextFactory>
            <UPJNDI>MARE/UP</UPJNDI>
			<JAASGroupName>mare</JAASGroupName>
			<RemoteLoginJNDI>MARE/REMOTELOGIN</RemoteLoginJNDI>
            <passwordEncryption>none</passwordEncryption>
            <!--userManager>es.indra.sicc.mgu.UserManagerSICC</userManager>
            <roleManager>es.indra.sicc.mgu.RoleManagerSICC</roleManager>
            <propertyManager>es.indra.sicc.mgu.PropertyManagerSICC</propertyManager>
            <userID>es.indra.sicc.mgu.UserIDSICC</userID-->
            <userManager>
                <class>es.indra.sicc.mgu.UserManagerSICC</class>
                <passwordEncryption>SHA</passwordEncryption>
                <expireTime>60</expireTime>
                <passwordHistorySize>5</passwordHistorySize>
                <passwordMinSize>1</passwordMinSize>
                <maxRetries>3</maxRetries> 
            </userManager>
            <roleManager>
                <class>es.indra.sicc.mgu.RoleManagerSICC</class>
                <rolesBatchSize>30</rolesBatchSize>
            </roleManager>
            <propertyManager>
                <class>es.indra.sicc.mgu.PropertyManagerSICC</class>
                <propertiesBatchSize>30</propertiesBatchSize>
            </propertyManager>
            <userID>es.indra.sicc.mgu.UserIDSICC</userID>
            <propertiesIncrementalThreshold>20</propertiesIncrementalThreshold>
            <rolesIncrementalThreshold>20</rolesIncrementalThreshold>
            <datasource>jdbc/mguDS</datasource>
            <db>
                <pool>${services.JDBCService.db.pool}</pool>
                <url>${services.JDBCService.db.url}</url>
                <driver>${services.JDBCService.db.driver}</driver>
                <user>own_mare</user>
                <password>own_mare</password>
            </db>
        </MGUService>
        <MSOService>
            <class>es.indra.mare.common.mii.services.mso.MSOService</class>
            <file>persistence/database.xml</file>
            <dbName>SICCD</dbName>
            <transactionManager>java:comp/UserTransaction</transactionManager>
        </MSOService>
        <MailService>
            <class>es.indra.mare.common.mii.services.mailservice.MailService</class>
            <hostName>smtp.telefonica.net</hostName>
            <from>mare@indra.es</from>
        </MailService>
        <XMLService>
            <class>es.indra.mare.common.mii.services.xmlservice.XMLService</class>
        </XMLService>
        <RuleService>
            <class>es.indra.mare.common.mii.services.rules.RuleService</class>
            <jsr94Provider>org.drools.jsr94.rules.RuleServiceProviderImpl</jsr94Provider>
            <providerUri>http://drools.org/</providerUri>
            <variable_1>reglasCalificacion.rules</variable_1>
            <variable_2>reglasPuntuacion.rules</variable_2>
            <variable_3>reglasEvaluacion.rules</variable_3>
            <variable_4>reglasCalificacionPremiacion.rules</variable_4>
            <variable_5>reglasTransformarPuntosAPremios.rules</variable_5>
            <variable_6>reglasDeterminarGanadoraRecomendacion.rules</variable_6>
            <variable_7>reglasDeterminarGanadoraRecomendacionGerente.rules</variable_7>
            <variable_8>reglasCalculoMetas.rules</variable_8>
        </RuleService>
        <LocalizationService>
            <class>es.indra.mare.common.mii.services.localization.LocalizationService</class>
            <baseName>SICCErrores</baseName>
        </LocalizationService>
    </services>
</mare>