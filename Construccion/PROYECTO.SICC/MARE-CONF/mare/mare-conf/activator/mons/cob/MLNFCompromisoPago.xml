<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		//System.out.println(">>>> Ingreso a Pre Activacion en MLNFCompromisoPago");
	</pre-activation-actions>
	<args-values>dts.getProperty("DTO_INPUT");</args-values>
	<activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<args-types>es.indra.sicc.dtos.cob.DTOEBuscarCompromisosPago</args-types>
		<method-name>compromisoPago</method-name>
		<class-name>es.indra.sicc.logicanegocio.cob.MONGestionesDeCobranza</class-name>
		<home-class>es.indra.sicc.logicanegocio.cob.MONGestionesDeCobranzaHome</home-class>
		<jndi-name>MONGestionesDeCobranza</jndi-name>
	</activator>
	<post-activation-actions>
		dts.setProperty("DTO_OUTPUT", dts.getLastResult() );
		//System.out.println(">>>> Salio de Post Activacion en MLNFCompromisoPago");
	</post-activation-actions>
</ejb-operation>
