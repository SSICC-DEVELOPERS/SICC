<?xml version="1.0" encoding="UTF-8"?>
<ejb-operation>
	<pre-activation-actions>
		System.out.println("Invocando CccSubpr.QueryFromToUserPage...");
    </pre-activation-actions>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("cccSubprFrom")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("cccSubprTo")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("userProperties")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pageCount")</args-values>
    <args-values>dts.getProperty("DTO_INPUT").getProperty("pageSize")</args-values>
    <activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <method-name>query</method-name>
		<args-types>es.indra.belcorp.mso.CccSubprData</args-types>
		<args-types>es.indra.belcorp.mso.CccSubprData</args-types>
		<args-types>java.util.HashMap</args-types>
		<args-types>java.lang.Integer</args-types>
		<args-types>java.lang.Integer</args-types>
        <class-name>es.indra.sicc.logicanegocio.ccc.MONCccProceSubproce</class-name>
        <home-class>es.indra.sicc.logicanegocio.ccc.MONCccProceSubproceHome</home-class>
        <jndi-name>MONCccProceSubproce</jndi-name>
    </activator>
    <post-activation-actions>
		dts.getProperty("DTO_OUTPUT").setProperty("result", dts.getLastResult());
		System.out.println("CccSubpr.QueryFromToUserPage invocado correctamente");
    </post-activation-actions>
</ejb-operation>
