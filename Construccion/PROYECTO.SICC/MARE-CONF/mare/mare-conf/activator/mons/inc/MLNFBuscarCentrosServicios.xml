<?xml version="1.0" encoding="ISO-8859-1"?>
<ejb-operation>
	<pre-activation-actions>
		//System.out.println("######### Ingresando a MLNFBuscarCentrosServicios #########");
	</pre-activation-actions>
	<args-values>dts.getProperty("DTO_INPUT")</args-values>
	<activator static-method="false" xsi:type="java:es.indra.mare.common.activator.EjbActivator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<class-name>es.indra.sicc.logicanegocio.inc.MONMantenimientoCentrosServicios</class-name>
		<home-class>es.indra.sicc.logicanegocio.inc.MONMantenimientoCentrosServiciosHome</home-class>
		<jndi-name>MONMantenimientoCentrosServicios</jndi-name>
		<method-name>buscarCentrosServicios</method-name>
		<args-types>es.indra.sicc.dtos.inc.DTOCentroServicio</args-types>
	</activator>
	<post-activation-actions>
		//System.out.println("######### Saliendo de MLNFBuscarCentrosServicios #########");
		dts.setProperty("DTO_OUTPUT", dts.getLastResult());
	</post-activation-actions>
</ejb-operation>