<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="MsgMensaDestiTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad MsgMensaDesti">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		
    	
		<CAMPO NOMBRE="fecMensDest" TIPO="STRING" LONGITUD="7"/>
		
		
		
		
    	
		<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="clieOidClie" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="codActi" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="fecMensDest" TIPO="STRING" LONGITUD="7"/>
			<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="clieOidClie" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="codActi" TIPO="STRING" LONGITUD="1"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
