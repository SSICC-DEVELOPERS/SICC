<CONECTOR  TIPO="EJB"  NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" OBSERVACIONES="ConectorBuscarPerfiles">
    <ENTRADA>
        <CAMPO NOMBRE="DTO" TIPO="OBJETO" LONGITUD="50"/>
        <CAMPO NOMBRE="businessID" TIPO="OBJETO" LONGITUD="50"/>
    </ENTRADA>
    <SALIDA>        
        <ROWSET NOMBRE="dtoSalida.resultado" FORMATO="MARETABLA" LONGITUD="50">          
          <CAMPO NOMBRE="OID" TIPO="STRING" LONGITUD="30"/>
          <CAMPO NOMBRE="NOMBRE_PERFIL" TIPO="STRING" LONGITUD="30"/>
      </ROWSET>
    </SALIDA>    
</CONECTOR>
