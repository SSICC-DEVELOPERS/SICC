<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="CobUsuarCobraViewTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad CobUsuarCobraView">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		<CAMPO NOMBRE="fecUltiEsta" TIPO="STRING" LONGITUD="7"/>
		<CAMPO NOMBRE="indJefeCobr" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indUsuaSupe" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indUsuaExte" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="valTurnHoraEntr" TIPO="STRING" LONGITUD="20"/>
		<CAMPO NOMBRE="valTurnHoraSali" TIPO="STRING" LONGITUD="20"/>
		<CAMPO NOMBRE="valObse" TIPO="STRING" LONGITUD="100"/>
		<CAMPO NOMBRE="userOidUsuaCobr" TIPO="STRING" LONGITUD="5"/>
		<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="fecUltiEsta" TIPO="STRING" LONGITUD="7"/>
			<CAMPO NOMBRE="indJefeCobr" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indUsuaSupe" TIPO="STRING" LONGITUD="1"/>
