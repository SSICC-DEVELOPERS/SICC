<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad CraActiv">

   <ENTRADA>
      <CAMPO NOMBRE="MMGCraActivQueryID" TIPO="OBJETO" LONGITUD="50"/>
	    <CAMPO NOMBRE="MMGCraActivQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
   </ENTRADA>

   <SALIDA>
      <ROWSET NOMBRE="result" FORMATO="CraActivFormFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="codActi" TIPO="STRING" LONGITUD="2"/>
			
				<CAMPO NOMBRE="numDiasDesp" TIPO="STRING" LONGITUD="5"/>
			
				<CAMPO NOMBRE="numCampAnte" TIPO="STRING" LONGITUD="2"/>
			
				<CAMPO NOMBRE="indEbel" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="indFest" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="indLabo" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="Descripcion" TIPO="STRING" LONGITUD="50"/>
			
				<!--El campo con los datos del tmestamp -->
				<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
			</ROWSET>
		</SALIDA>
</CONECTOR>
