<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad MsgMetac">

   <ENTRADA>
		<CAMPO NOMBRE="MMGMsgMetacQueryID" TIPO="OBJETO" LONGITUD="50"/>
		<CAMPO NOMBRE="MMGMsgMetacQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="MsgMetacListFormatter" LONGITUD="50">
			<!--El campo con el id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>

			<!--Los campos de la lista-->
			
				<CAMPO NOMBRE="codMeta" TIPO="STRING" LONGITUD="6"/>
			
				<CAMPO NOMBRE="Descripcion" TIPO="STRING" LONGITUD="40"/>
			
				<CAMPO NOMBRE="timeOidTipo" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="valEsta" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="valLiteIden" TIPO="STRING" LONGITUD="240"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
