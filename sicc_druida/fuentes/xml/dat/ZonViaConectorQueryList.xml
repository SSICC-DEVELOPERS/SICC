<CONECTOR TIPO="EJB" NOMBRE="BusinessFacadeBean" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad ZonVia">

   <ENTRADA>
		<CAMPO NOMBRE="MMGZonViaQueryID" TIPO="OBJETO" LONGITUD="50"/>
		<CAMPO NOMBRE="MMGZonViaQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="es.indra.druida.belcorp.ZonViaListFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="indActiVia" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="nomVia" TIPO="STRING" LONGITUD="40"/>
			
				<CAMPO NOMBRE="tiviOidTipoVia" TIPO="STRING" LONGITUD="12"/>
			
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
