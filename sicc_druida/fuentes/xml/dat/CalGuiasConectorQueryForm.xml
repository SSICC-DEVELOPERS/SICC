<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad CalGuias">

   <ENTRADA>
      <CAMPO NOMBRE="MMGCalGuiasQueryID" TIPO="OBJETO" LONGITUD="50"/>
	    <CAMPO NOMBRE="MMGCalGuiasQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
   </ENTRADA>

   <SALIDA>
      <ROWSET NOMBRE="result" FORMATO="CalGuiasFormFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="codGuia" TIPO="STRING" LONGITUD="4"/>
			
				<CAMPO NOMBRE="dpteOidDepa" TIPO="STRING" LONGITUD="12"/>
			
