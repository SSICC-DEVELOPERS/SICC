		<CAMPO NOMBRE="MMGCraPerioQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="CraPerioListFormatter" LONGITUD="50">
