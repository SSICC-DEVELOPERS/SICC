				<CAMPO NOMBRE="periOidPeri" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="fecInic" TIPO="STRING" LONGITUD="10"/>
			
				<CAMPO NOMBRE="fecFina" TIPO="STRING" LONGITUD="7"/>
			
				<CAMPO NOMBRE="valEsta" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="valNombPeri" TIPO="STRING" LONGITUD="40"/>
			
				<CAMPO NOMBRE="indPeriCort" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="indPeriCruc" TIPO="STRING" LONGITUD="1"/>
			
