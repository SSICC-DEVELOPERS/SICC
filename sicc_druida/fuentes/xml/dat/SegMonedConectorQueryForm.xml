<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad SegMoned">

   <ENTRADA>
      <CAMPO NOMBRE="MMGSegMonedQueryID" TIPO="OBJETO" LONGITUD="50"/>
	    <CAMPO NOMBRE="MMGSegMonedQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
   </ENTRADA>

   <SALIDA>
      <ROWSET NOMBRE="result" FORMATO="SegMonedFormFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="codMone" TIPO="STRING" LONGITUD="3"/>
			
				<CAMPO NOMBRE="Descripcion" TIPO="STRING" LONGITUD="30"/>
			
				<CAMPO NOMBRE="valNombCortMone" TIPO="STRING" LONGITUD="3"/>
			
				<CAMPO NOMBRE="valSimbMone" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="numDeci" TIPO="STRING" LONGITUD="1"/>
			
				<!--El campo con los datos del tmestamp -->
				<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
			</ROWSET>
		</SALIDA>
</CONECTOR>
