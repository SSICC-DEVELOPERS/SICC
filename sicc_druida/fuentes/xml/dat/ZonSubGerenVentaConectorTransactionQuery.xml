<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="ZonSubGerenVentaTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad ZonSubGerenVenta">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
		<CAMPO NOMBRE="marcOidMarc" TIPO="STRING" LONGITUD="12"/>
		<CAMPO NOMBRE="canaOidCana" TIPO="STRING" LONGITUD="12"/>
		<CAMPO NOMBRE="codSubgVent" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="indBorr" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indActi" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="codNse1" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="codNse2" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="codNse3" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="desSubgVent" TIPO="STRING" LONGITUD="40"/>
		<CAMPO NOMBRE="clieOidClie" TIPO="STRING" LONGITUD="12"/>
		<CAMPO NOMBRE="fecRezo" TIPO="STRING" LONGITUD="7"/>
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="marcOidMarc" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="canaOidCana" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="codSubgVent" TIPO="STRING" LONGITUD="2"/>
			<CAMPO NOMBRE="indBorr" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indActi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="codNse1" TIPO="STRING" LONGITUD="2"/>
			<CAMPO NOMBRE="codNse2" TIPO="STRING" LONGITUD="2"/>
			<CAMPO NOMBRE="codNse3" TIPO="STRING" LONGITUD="2"/>
			<CAMPO NOMBRE="desSubgVent" TIPO="STRING" LONGITUD="40"/>
			<CAMPO NOMBRE="clieOidClie" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="fecRezo" TIPO="STRING" LONGITUD="7"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
