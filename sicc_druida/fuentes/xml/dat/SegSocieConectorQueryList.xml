<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad SegSocie">

   <ENTRADA>
		<CAMPO NOMBRE="MMGSegSocieQueryID" TIPO="OBJETO" LONGITUD="50"/>
		<CAMPO NOMBRE="MMGSegSocieQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="SegSocieListFormatter" LONGITUD="50">
			<!--El campo con el id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>

			<!--Los campos de la lista-->
			
				<CAMPO NOMBRE="codSoci" TIPO="STRING" LONGITUD="4"/>
			
				<CAMPO NOMBRE="valDeno" TIPO="STRING" LONGITUD="40"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
