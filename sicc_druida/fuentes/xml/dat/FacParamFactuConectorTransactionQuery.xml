<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="FacParamFactuTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad FacParamFactu">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		<CAMPO NOMBRE="indDocuMonoPagi" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="valMontLimiUit" TIPO="STRING" LONGITUD="9"/>
		<CAMPO NOMBRE="numMaxiCampPrd" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="indTipoProyRegi" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indMostVencCupo" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="numMaxiPediProy" TIPO="STRING" LONGITUD="5"/>
		<CAMPO NOMBRE="codModaDist" TIPO="STRING" LONGITUD="1"/>
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="indDocuMonoPagi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="valMontLimiUit" TIPO="STRING" LONGITUD="9"/>
			<CAMPO NOMBRE="numMaxiCampPrd" TIPO="STRING" LONGITUD="2"/>
			<CAMPO NOMBRE="indTipoProyRegi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indMostVencCupo" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="numMaxiPediProy" TIPO="STRING" LONGITUD="5"/>
			<CAMPO NOMBRE="codModaDist" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indCheqClieNuev" TIPO="STRING" LONGITUD="1"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
