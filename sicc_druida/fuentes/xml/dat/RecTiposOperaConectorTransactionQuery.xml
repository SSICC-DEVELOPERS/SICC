<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="RecTiposOperaTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad RecTiposOpera">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		<CAMPO NOMBRE="valTipoOper" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="indCampRefeUnic" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="numDiasHaciAtra" TIPO="STRING" LONGITUD="2"/>
		<CAMPO NOMBRE="valInfoEbelNoti" TIPO="STRING" LONGITUD="1"/>
