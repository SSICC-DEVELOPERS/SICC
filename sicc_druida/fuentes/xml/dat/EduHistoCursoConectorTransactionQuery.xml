<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="EduHistoCursoTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad EduHistoCurso">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		
    	
		<CAMPO NOMBRE="ctsuOidClieTipoSubt" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="mcurOidCurs" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indApta" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indInvit" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indAsis" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="ctsuOidClieTipoSubt" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="mcurOidCurs" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="indApta" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indInvit" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indAsis" TIPO="STRING" LONGITUD="1"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
