<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="EduPlantCursoDetalTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad EduPlantCursoDetal">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		
    	
		<CAMPO NOMBRE="cplcOidCabe" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="valDeta" TIPO="STRING" LONGITUD="50"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indVisi" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indModi" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indObli" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="cplcOidCabe" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="valDeta" TIPO="STRING" LONGITUD="50"/>
			<CAMPO NOMBRE="indVisi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indModi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indObli" TIPO="STRING" LONGITUD="1"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
