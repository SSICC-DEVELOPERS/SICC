<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad SegCanalView">

   <ENTRADA>
		<CAMPO NOMBRE="MMGSegCanalViewQueryID" TIPO="OBJETO" LONGITUD="50"/>
		<CAMPO NOMBRE="MMGSegCanalViewQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="SegCanalViewListFormatter" LONGITUD="50">
			<!--El campo con el id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>

			<!--Los campos de la lista-->
			
				<CAMPO NOMBRE="codCana" TIPO="STRING" LONGITUD="3"/>
			
				<CAMPO NOMBRE="Descripcion" TIPO="STRING" LONGITUD="30"/>
			
				<CAMPO NOMBRE="tipeOidTipoPeri" TIPO="STRING" LONGITUD="12"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
