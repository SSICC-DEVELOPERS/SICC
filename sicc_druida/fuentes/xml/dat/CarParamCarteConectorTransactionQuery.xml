		<CAMPO NOMBRE="numDiasDeudVenc" TIPO="STRING" LONGITUD="3"/>
		
		
		
		
    	
		<CAMPO NOMBRE="indLineCred" TIPO="STRING" LONGITUD="1"/>
		
		
		
		
    	
