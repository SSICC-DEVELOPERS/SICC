<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad CccBanco">

   <ENTRADA>
      <CAMPO NOMBRE="MMGCccBancoQueryID" TIPO="OBJETO" LONGITUD="50"/>
	    <CAMPO NOMBRE="MMGCccBancoQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
   </ENTRADA>

   <SALIDA>
      <ROWSET NOMBRE="result" FORMATO="CccBancoFormFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="codBanc" TIPO="STRING" LONGITUD="4"/>
			
				<CAMPO NOMBRE="desBanc" TIPO="STRING" LONGITUD="40"/>
			
				<CAMPO NOMBRE="codBrio" TIPO="STRING" LONGITUD="6"/>
			
				<CAMPO NOMBRE="codSwifInfo" TIPO="STRING" LONGITUD="20"/>
			
				<CAMPO NOMBRE="valObsBanc" TIPO="STRING" LONGITUD="100"/>
			
				<!--El campo con los datos del tmestamp -->
				<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
			</ROWSET>
		</SALIDA>
</CONECTOR>
