<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="BelTipoMovimAlmacViewTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad BelTipoMovimAlmacView">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
		<CAMPO NOMBRE="codTipoMovi" TIPO="STRING" LONGITUD="3"/>
		<CAMPO NOMBRE="codMoviSap" TIPO="STRING" LONGITUD="18"/>
		<CAMPO NOMBRE="indMoviEntrAlma" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indMantMovi" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="indSap" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="codOper1" TIPO="STRING" LONGITUD="1"/>
		<CAMPO NOMBRE="codOper2" TIPO="STRING" LONGITUD="1"/>
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="codTipoMovi" TIPO="STRING" LONGITUD="3"/>
			<CAMPO NOMBRE="codMoviSap" TIPO="STRING" LONGITUD="18"/>
			<CAMPO NOMBRE="indMoviEntrAlma" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indMantMovi" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="indSap" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="codOper1" TIPO="STRING" LONGITUD="1"/>
			<CAMPO NOMBRE="codOper2" TIPO="STRING" LONGITUD="1"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
