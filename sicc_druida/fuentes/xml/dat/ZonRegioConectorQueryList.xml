		<CAMPO NOMBRE="MMGZonRegioQueryID" TIPO="OBJETO" LONGITUD="50"/>
		<CAMPO NOMBRE="MMGZonRegioQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
	</ENTRADA>

	<SALIDA>
    	<ROWSET NOMBRE="result" FORMATO="ZonRegioListFormatter" LONGITUD="50">
			<!--El campo con el id de la entidad-->
