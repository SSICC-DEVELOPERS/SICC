			<!--Los campos de la lista-->
			
				<CAMPO NOMBRE="codRegi" TIPO="STRING" LONGITUD="2"/>
			
				<CAMPO NOMBRE="indActi" TIPO="STRING" LONGITUD="1"/>
			
