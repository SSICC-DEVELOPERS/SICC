<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="CarEjecuCuentTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad CarEjecuCuent">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		
    	
		<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="codEjecCuen" TIPO="STRING" LONGITUD="20"/>
		
		
		
		
    	
		<CAMPO NOMBRE="jeraOidJera" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<!--El id de la entidad-->
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
			<!--Los campos de modo listado-->
			<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="codEjecCuen" TIPO="STRING" LONGITUD="20"/>
			<CAMPO NOMBRE="jeraOidJera" TIPO="STRING" LONGITUD="12"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
