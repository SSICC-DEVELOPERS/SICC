<CONECTOR TIPO="DRUIDATRANSACTION" REVISION="3.1" NOMBRE="MsgMensaUnidaAdminTransactionQuery" 
	OBSERVACIONES="Conector transaccional para la ejección de query sobre la entidad MsgMensaUnidaAdmin">
   <ENTRADA>
		<!--Los atributos de la entidad-->
		
    	
		<CAMPO NOMBRE="codNive" TIPO="STRING" LONGITUD="10"/>
		
		
		
		
    	
		<CAMPO NOMBRE="zorgOidRegi" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="zzonOidZona" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="terrOidTerr" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="zsccOidSecc" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
    	
		<CAMPO NOMBRE="mensOidMens" TIPO="STRING" LONGITUD="12"/>
		
		
		
		
		<!--Los parametros para la paginación-->
		<CAMPO NOMBRE="pageCount" TIPO="STRING" LONGITUD="30"/>
		<CAMPO NOMBRE="pageSize" TIPO="STRING" LONGITUD="30"/>
   </ENTRADA>

	<SALIDA>
		<ROWSET NOMBRE="result" LONGITUD="50">
			<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="codNive" TIPO="STRING" LONGITUD="10"/>
			<CAMPO NOMBRE="zorgOidRegi" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="zzonOidZona" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="terrOidTerr" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="zsccOidSecc" TIPO="STRING" LONGITUD="12"/>
			<CAMPO NOMBRE="mensOidMens" TIPO="STRING" LONGITUD="12"/>
			
			<!--El campo con los datos del tmestamp -->
			<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
		</ROWSET>
	</SALIDA>
</CONECTOR>
