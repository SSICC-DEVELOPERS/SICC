<CONECTOR TIPO="EJB" NOMBRE="mare.mln.BusinessFacade" METODO="execute" REVISION="3.1" 
	OBSERVACIONES="Conector para la consulta sobre la entidad BelCaja">

   <ENTRADA>
      <CAMPO NOMBRE="MMGBelCajaQueryID" TIPO="OBJETO" LONGITUD="50"/>
	    <CAMPO NOMBRE="MMGBelCajaQueryDTO" TIPO="OBJETO" LONGITUD="50"/>
   </ENTRADA>

   <SALIDA>
      <ROWSET NOMBRE="result" FORMATO="BelCajaFormFormatter" LONGITUD="50">
			
				<CAMPO NOMBRE="id" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="paisOidPais" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="sbacOidSbac" TIPO="STRING" LONGITUD="12"/>
			
				<CAMPO NOMBRE="codCaja" TIPO="STRING" LONGITUD="3"/>
			
				<CAMPO NOMBRE="valEstaCaja" TIPO="STRING" LONGITUD="1"/>
			
				<CAMPO NOMBRE="fecCier" TIPO="STRING" LONGITUD="10"/>
			
				<CAMPO NOMBRE="userOidUser" TIPO="STRING" LONGITUD="5"/>
			
				<CAMPO NOMBRE="ticjOidTipoCaja" TIPO="STRING" LONGITUD="12"/>
			
				<!--El campo con los datos del tmestamp -->
				<CAMPO NOMBRE="timestamp" TIPO="STRING" LONGITUD="30"/>
			</ROWSET>
		</SALIDA>
</CONECTOR>
