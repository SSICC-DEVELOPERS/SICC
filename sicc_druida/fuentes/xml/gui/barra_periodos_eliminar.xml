<!DOCTYPE PAGINA SYSTEM "dtd/druida_3_0.dtd">

<PAGINA nombre="barra_periodos_eliminar" titulo="Eliminar Períodos" estilos="estilos.css" colorf="#F0F0F0"
        msgle="Eliminar Periodos" onload="" xml:lang="es">
<JS src="sicc_util.js;mensajes-es.js"/>

 <JAVASCRIPT>
   <![CDATA[
	
	var APP_CONTENIDO = parent.frames['contenido']; 

    ]]>
   </JAVASCRIPT>

 <SCRIPT LANGUAGE="javascript" FOR="document" EVENT="onselectstart">
 return false;

</SCRIPT>
 
<SCRIPT LANGUAGE="javascript" FOR="document" EVENT="ondragstart">
 return false;
</SCRIPT>

<FORMULARIO nombre="frmEliminarPeriodo">
	<table width="100%" height="23" bgcolor="#496A9A" cellpadding="0" cellspacing="0" border="0">
	  <tr>
		 <td><IMG src="flechaTitle.jpg" width="38" height="23" /></td>
     
   		 <td class="titulos" valign="middle" width="100%">  
   		 	<LABELC nombre="lblTitulo" ancho="300" alto="23" filas="1" id="titulos" valor="Eliminar Períodos" />
		</td>

		 <td>
			<BOTONERA nombre="btnBarra" tipo="H" x="0" y="0">
				<BTN funcion="APP_CONTENIDO.Save()" off="iconGuardar.gif" 	on="iconGuardar.gif" 	texto="Insertar" 	estado="activo"/>
				<BTN on="iconVolverOff.gif"		off="iconVolverOff.gif"		funcion="APP_CONTENIDO.fVolver();"           	texto="Volver"    estado="inactivo"/>
				<BTN on="iconSalir.gif"			off="iconSalir.gif"		funcion="APP_CONTENIDO.fSalir();"		texto="Salir"	  estado="activo"/>
