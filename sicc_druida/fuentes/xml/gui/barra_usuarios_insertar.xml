            <BTN on="iconPais.gif"      off="iconPais.gif"      funcion="APP_CONTENIDO.fPais();"          texto="Pais"      estado="activo"/>
            <BTN on="iconAyuda.gif"     off="iconAyuda.gif"     funcion="APP_CONTENIDO.fAyuda();"         texto="Ayuda"     estado="activo"/>
