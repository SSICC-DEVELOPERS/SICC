    function onLoad(){
      
      configurarMenuSecundario('Formulario');
      fMostrarMensajeError();
      setearPais();
      cambioPais();
      focaliza('Formulario.cbPais');
      accion('Formulario.cbMoneda', '.disabled = true');
    
