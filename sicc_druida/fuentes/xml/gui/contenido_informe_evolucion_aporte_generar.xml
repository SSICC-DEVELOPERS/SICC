   function quitarItemVacio(elemento){
      var elementoADevolver = new Array();
     
      for(var k=0;k<elemento.length;k++) {
          if (elemento[k] != ''){                            
             elementoADevolver[elementoADevolver.length] = elemento[k];
          }
      }
         
      return elementoADevolver;
      
   }

//------------------------------------------------------------------------------

  function generar(formato){
      
    if(sicc_validaciones_generales()){
      
      var a = new Object();
      a.accion = 'generar';
      
      a.pais = get('Formulario.cbPais');
      a.marca = get('Formulario.cbMarca');
      a.marcaDesc = get('Formulario.cbMarca','T');
      a.canal = get('Formulario.cbCanal');
      a.subgerenciaVentas = quitarItemVacio(get('Formulario.cbSubgerenciaVenta'));
      a.subgerenciaVentasDesc = quitarItemVacio(get('Formulario.cbSubgerenciaVenta','T'));
      a.region = quitarItemVacio(get('Formulario.cbRegion'));
      a.regionDesc = quitarItemVacio(get('Formulario.cbRegion','T'));
      a.zona = quitarItemVacio(get('Formulario.cbZona'));
      a.zonaDesc = quitarItemVacio(get('Formulario.cbZona','T'));
      a.tipoVenta = get('Formulario.cbTipoVenta');
      a.tipoVentaDesc = get('Formulario.cbTipoVenta','T');
      a.tipoOferta = quitarItemVacio(get('Formulario.cbTiposOferta'));
      a.tipoCliente = quitarItemVacio(get('Formulario.cbTipoCliente'));
      a.subtipoCliente = quitarItemVacio(get('Formulario.cbSubtipoCliente'));
      a.tipoClasificacion = quitarItemVacio(get('Formulario.cbTipoClasificacionCliente'));
      a.clasificacion = quitarItemVacio(get('Formulario.cbClasificacionCliente'));
      a.tipoAporte = get('Formulario.cbTipoAporte');
      a.percentil = quitarItemVacio(get('Formulario.cbPercentil'));
      a.moneda = get('Formulario.cbMoneda');
      a.monedaDesc = get('Formulario.cbMoneda','T');
      a.unidadNegocio = quitarItemVacio(get('Formulario.cbUnidadNegocio'));
      a.negocio = quitarItemVacio(get('Formulario.cbNegocio'));
      a.anio = get('Formulario.txtAnio');
      a.formato = formato;    
      
      mostrarModalSICC('INCEvolucionAporte','generar', a);
      
      }
    
  }

