                  txt_to('lbldtTotalPuntaje', get("frmFormulario.totalPuntaje"));
                  txt_to('lbldtCodDuplacyz', get("frmFormulario.codDuplaCyz"));
                  txt_to('lbldtNombreDuplacyz', get("frmFormulario.nombreDuplaCyz"));

