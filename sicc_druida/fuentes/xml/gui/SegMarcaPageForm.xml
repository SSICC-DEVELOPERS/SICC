<!-- TablaRoot -->
<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
	
	<!--FilaGapRoot -->
	<TR> 
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="12" /></TD>
		<TD align="center" width="750"><IMG src="b.gif" width="12" height="12" /></TD>
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="12" /></TD>
	</TR>

	<!--FilaFormiulario-->
	<TR> 
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="8" /></TD>
		<TD width="100%">
			
			<!--FieldSetFormulaio-->
			<fieldset>
	    		<legend class="legend">
	    			<LABELC nombre="legendLbl" alto="13" filas="1" id="legend" valor="" cod="SegMarca.legend.label"/>
	    		</legend>
				<TABLE width="100%" border="0" align="center" cellspacing="0" cellpadding="0">
					<TR>
						<TD width="100%" nowrap="true" class="datosCampos" colspan="3">
							<IMG src="b.gif" width="8" height="8"/>
						</TD>
					</TR>

					<!--Aqui comienzan los trs del formulario-->
					<TR nombre="formTr0">
						<TD align="center" width="8"><IMG src="b.gif" width="8" height="12" /></TD>						
						<TD width="100%">
							<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
								<TR align="left">
									<TD nombre="idTdId">
										<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
											<TR>
												<TD valign="top" height="13">
													<LABELC nombre="lblId" alto="13" filas="1" id="datosTitle" cod="SegMarca.id.label" />
												</TD>
												
											</TR>
											<TR nombre="idWidgetTrId">
												<TD align="left" nowrap="true">
													<TEXT nombre="id" id="datosCampos" trim="S"
														max="12" onchange="" req="N" size="12" valor="" validacion="" disabled=""
														onblur=""
														ontab="ejecutarAccionFoco('segMarcaFrm',1, true)" onshtab="ejecutarAccionFoco('segMarcaFrm', 1, false)"/>
												</TD>
												
											</TR>
										</TABLE>
									</TD>

									<TD nombre="idGapTdId" nowrap="true" class="datosCampos" >
										<IMG src="b.gif" width="25" height="8"/>
									</TD>
									
									
								
									<!--Celda para ajustar todos los elementos-->
									<TD width="100%"><IMG src="b.gif" /></TD>	
								</TR>
							</TABLE>
						</TD>

					</TR>
					
					<!-- Fila para espaciar entr fila y fila de campo -->
					<TR nombre="formGapTr0">
						<TD align="center" width="8"><IMG src="b.gif" width="12" height="8" /></TD>
						<TD><IMG src="b.gif" width="8" height="8"/></TD>
						<TD align="center" width="8"><IMG src="b.gif" width="12" height="8" /></TD>
					</TR>
					
					<TR nombre="formTr1">
						<TD align="center" width="8"><IMG src="b.gif" width="8" height="12" /></TD>
						<TD width="100%">
							<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
								<TR align="left">
									<TD nombre="codMarcTdId">
										<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
											<TR>
												<TD valign="top" height="13">
													<LABELC nombre="lblCodMarc" alto="13" filas="1" id="datosTitle" cod="SegMarca.codMarc.label" />
												</TD>
												
											</TR>
											<TR nombre="codMarcWidgetTrId">
												<TD align="left" nowrap="true">
													<TEXT nombre="codMarc" id="datosCampos" trim="S"
														max="3" onchange="" req="N" size="3" valor="" validacion="" disabled=""
														onblur=""
														ontab="ejecutarAccionFoco('segMarcaFrm',2, true)" onshtab="ejecutarAccionFoco('segMarcaFrm', 2, false)"/>
												</TD>
												
											</TR>
										</TABLE>
									</TD>

									<TD nombre="codMarcGapTdId" nowrap="true" class="datosCampos" >
										<IMG src="b.gif" width="25" height="8"/>
									</TD>
									
									<TD nombre="desMarcTdId">
										<TABLE width="100%" border="0" cellspacing="0" cellpadding="0" align="left">
											<TR>
												<TD valign="top" height="13">
													<LABELC nombre="lblDesMarc" alto="13" filas="1" id="datosTitle" cod="SegMarca.desMarc.label" />
												</TD>
												
											</TR>
											<TR nombre="desMarcWidgetTrId">
												<TD align="left" nowrap="true">
													<TEXT nombre="desMarc" id="datosCampos" trim="S"
														max="30" onchange="" req="N" size="30" valor="" validacion="" disabled=""
														onblur=""
														ontab="ejecutarAccionFoco('segMarcaFrm',3, true)" onshtab="ejecutarAccionFoco('segMarcaFrm', 3, false)"/>
												</TD>
												
											</TR>
										</TABLE>
									</TD>

									<TD nombre="desMarcGapTdId" nowrap="true" class="datosCampos" >
										<IMG src="b.gif" width="25" height="8"/>
									</TD>
									
									

									<!--Celda para ajustar todos los elementos-->
									<TD width="100%"><IMG src="b.gif" /></TD>	
								</TR>
							</TABLE>
						</TD>
						<TD width="100%"></TD>
					</TR>

					<!-- Fila para espaciar la última fila del borde inferior del fileset-->
					<TR nombre="formGapTr1">
						<TD align="center" width="8"><IMG src="b.gif" width="8" height="8" /></TD>
						<TD><IMG src="b.gif" width="8" height="8"/></TD>
						<TD align="center" width="8"><IMG src="b.gif" width="8" height="8" /></TD>
					</TR>
				</TABLE>
			</fieldset>
		</TD>
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="12" /></TD>
	</TR>
	
	<TR nombre="segMarcaTrButtons"> 
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="12" /></TD>
        <TD>
			<FIELDSET>
				<TABLE width="100%" border="0" align="center" cellspacing="0" cellpadding="0">        
					<TR>
						<TD class="tablaTitle" nombre="segMarcaTdQueryButton">
							<BOTON nombre="segMarcaQueryButton" cod="MMGGlobal.queryButton.label" estado="false" accion="segMarcaFirstPage();" tipo="html" ID="botonContenido">
							</BOTON>
						</TD>
						<TD class="tablaTitle" nombre="segMarcaTdLovNullSelectionButton" align="left" width="100%">
							<IMG src="b.gif" height="8" width="8" />
							<BOTON nombre="segMarcaLovNullButton" ID="botonContenido" tipo="html" accion="segMarcaLovNullSelectionAction();" estado="true" cod="MMGGlobal.lovNullButton.label"/>
						</TD>
					</TR>
				</TABLE>
			</FIELDSET>
		</TD>
		<TD align="center" width="12"><IMG src="b.gif" width="12" height="12" /></TD>
	</TR> 
</TABLE>

