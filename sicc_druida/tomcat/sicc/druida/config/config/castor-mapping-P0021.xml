         <sql name="OID_MOTI_RECH_DESB"/>
         <bind-xml name="id"/>
      </field>
      <field name="paisOidPais" type="es.indra.belcorp.mso.SegPaisViewData" required="true">
         <sql name="PAIS_OID_PAIS"/>
         <bind-xml name="paisOidPais"/>
      </field>
      <field name="codRechDesb" type="string" required="true">
         <sql name="COD_RECH_DESB"/>
         <bind-xml name="codRechDesb"/>
      </field>
   </class>
</mapping>