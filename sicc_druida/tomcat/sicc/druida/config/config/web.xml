<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" 
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
    <display-name>SICC/Druida</display-name>
    <description>Core Druida 3.0</description>

    <servlet>
        <servlet-name>sicc0000jg</servlet-name>
        <servlet-class>sicc0000jg</servlet-class>
    </servlet>    
	<servlet>
        <servlet-name>ServletApplet</servlet-name>
        <servlet-class>ServletApplet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>sicc0000jg</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletApplet</servlet-name>
        <url-pattern>/ServletApplet</url-pattern>
    </servlet-mapping>
</web-app>
